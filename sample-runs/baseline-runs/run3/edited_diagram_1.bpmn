      <dataInputAssociation id="DA_ApplicationEval"> <sourceRef>DORef_Application</sourceRef> </dataInputAssociation> <incoming>Flow_Start_Eval</incoming> <outgoing>Flow_EvalGW</outgoing> </task> <exclusiveGateway id="Gateway_ApplicationApproved" name="Application Approved?"> <incoming>Flow_EvalGW</incoming> <outgoing>Flow_GW_Approved</outgoing> <outgoing>Flow_GW_Rejected</outgoing> </exclusiveGateway> <!-- Rejected Path --> <task id="Task_NotifyRejection" name="Notify Rejection"> <incoming>Flow_GW_Rejected</incoming> <outgoing>Flow_NotifyRej_End</outgoing> </task> <endEvent id="EndEvent_ApplicationRejected" name="End (Rejected)"> <incoming>Flow_NotifyRej_End</incoming> </endEvent> <!-- Approved Path --> <task id="Task_CreateAccount" name="Create Bank Account"> <dataOutputAssociation id="DA_AccountOut"> <targetRef>DORef_Account</targetRef> </dataOutputAssociation> <incoming>Flow_GW_Approved</incoming> <outgoing>Flow_Create_Parallel</outgoing> </task> <parallelGateway id="Gateway_SendWelcomePackParallel" name="Send Welcome Materials"> <incoming>Flow_Create_Parallel</incoming> <outgoing>Flow_Pack_Welcome</outgoing> <outgoing>Flow_Pack_Card</outgoing> <outgoing>Flow_Pack_PIN</outgoing> </parallelGateway> <task id="Task_SendWelcomePack" name="Send Welcome Pack Letter">
