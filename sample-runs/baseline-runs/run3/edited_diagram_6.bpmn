<?xml version="1.0" encoding="UTF-8"?> <definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="Definitions_1" targetNamespace="http://bpmn.io/schema/bpmn"> 
  <!-- Pools --> <collaboration id="Collaboration_1"> <participant id="Participant_Customer" name="Student" processRef="Process_Student"/> <participant id="Participant_Institute" name="Sports Institute" processRef="Process_Institute"/> <messageFlow id="MF_CourseSelection" sourceRef="Event_SelectCourseRequest" targetRef="Event_SelectCourseRequest_Receive"/> <messageFlow id="MF_ActivationReq" sourceRef="Event_RequestActivation" targetRef="Event_ReceiveActivationReq"/> <messageFlow id="MF_ActivationResponse" sourceRef="Event_ActivationResponse_Send" targetRef="Event_ActivationResponse_Receive"/> <messageFlow id="MF_CourseTicket" sourceRef="Event_ReceiveCourseTicket_Send" targetRef="Event_ReceiveCourseTicket"/> </collaboration> 
  
  <!-- Process: Student --> <process id="Process_Student" isExecutable="true"> 
    <laneSet> <lane id="Lane_Student" name="Student"/> </laneSet> <startEvent id="StartEvent" name="Start Registration"/> <sequenceFlow id="SF1" sourceRef="StartEvent" targetRef="Event_SelectCourseRequest"/> <!-- Select Course --> <intermediateThrowEvent id="Event_SelectCourseRequest" name="Request Course Info"> <messageEventDefinition id="MED_SelectCourse"/> </intermediateThrowEvent> <sequenceFlow id="SF2" sourceRef="Event_SelectCourseRequest" targetRef="Event_SelectCourseRequest_Receive"/> <!-- Receive course info from Institute --> <intermediateCatchEvent id="Event_SelectCourseRequest_Receive" name="Receive Course/Slots Info"> <messageEventDefinition id="MED_ReceiveCourse"/> </intermediateCatchEvent> <sequenceFlow id="SF3" sourceRef="Event_SelectCourseRequest_Receive" targetRef="Task_SelectCourseDate"/> <task id="Task_SelectCourseDate" name="Select Course &amp; Date"/> <sequenceFlow id="SF4" sourceRef="Task_SelectCourseDate" targetRef="Gateway_HaveAccount"/> <!-- Account Check --> <exclusiveGateway id="Gateway_HaveAccount" name="Have Account?"/> <sequenceFlow id="SF5a" sourceRef="Gateway_HaveAccount" targetRef="Task_Login"> <conditionExpression xsi:type="tFormalExpression">yes</conditionExpression> </sequenceFlow> <sequenceFlow id="SF5b" sourceRef="Gateway_HaveAccount" targetRef="Gateway_UniversityRegistration"> <conditionExpression xsi:type="tFormalExpression">no</conditionExpression> </sequenceFlow> <exclusiveGateway id="Gateway_UniversityRegistration" name="Registered at Eligible University?"/> <sequenceFlow id="SF6a" sourceRef="Gateway_UniversityRegistration" targetRef="Task_RegisterAccount"> <conditionExpression xsi:type="tFormalExpression">yes</conditionExpression> </sequenceFlow> <sequenceFlow id="SF6b" sourceRef="Gateway_UniversityRegistration" targetRef="Event_RequestActivation"> <conditionExpression xsi:type="tFormalExpression">no</conditionExpression> </sequenceFlow> <!-- Register Account --> <task id="Task_RegisterAccount" name="Register Account"/> <sequenceFlow id="SF7" sourceRef="Task_RegisterAccount" targetRef="Task_Login"/> <!-- Request Activation from Institute --> <intermediateThrowEvent id="Event_RequestActivation" name="Request Activation"> <messageEventDefinition id="MED_Activation_Request"/> </intermediateThrowEvent> <sequenceFlow id="SF8" sourceRef="Event_RequestActivation" targetRef="Event_ActivationResponse_Receive"/> <intermediateCatchEvent id="Event_ActivationResponse_Receive" name="Receive Activation Response"> <messageEventDefinition id="MED_Activation_Response"/> </intermediateCatchEvent> <sequenceFlow id="SF9" sourceRef="Event_ActivationResponse_Receive" targetRef="Task_RegisterAccount"/> <!-- Login to account --> <task id="Task_Login" name="Log in to Account"/> <sequenceFlow id="SF10" sourceRef="Task_Login" targetRef="Task_TweetInvite"/> <!-- Parallel: Tweet to friends and Register for Course --> <parallelGateway id="PG_TweetRegister_Split"/> <sequenceFlow id="SF11" sourceRef="Task_TweetInvite" targetRef="PG_TweetRegister_Split"/> <sequenceFlow id="SF12a" sourceRef="PG_TweetRegister_Split" targetRef="Task_TweetToFriends"/> <sequenceFlow id="SF12b" sourceRef="PG_TweetRegister_Split" targetRef="Task_CompleteRegistration"/> <task id="Task_TweetInvite" name="Proceed to Registration"/> <task id="Task_TweetToFriends" name="Tweet to Friends"/> <task id="Task_CompleteRegistration" name="Complete Registration and Provide Payment"> <dataInputAssociation id="asso1"> <sourceRef>DataRef_CourseInfo</sourceRef> </dataInputAssociation> <dataInputAssociation id="asso2"> <sourceRef>DataRef_AccountInfo</sourceRef> </dataInputAssociation> </task> 
<dataObject id="Data_CourseInfo" />
<dataObjectReference id="DataRef_CourseInfo" dataObjectRef="Data_CourseInfo" name="Course Info"/>

<dataObject id="Data_AccountInfo"/>
<dataObjectReference id="DataRef_AccountInfo" dataObjectRef="Data_AccountInfo"  name="Account Info"/>

    <sequenceFlow id="SF13a" sourceRef="Task_TweetToFriends" targetRef="PG_TweetRegister_Join"/> <sequenceFlow id="SF13b" sourceRef="Task_CompleteRegistration" targetRef="PG_TweetRegister_Join"/> <parallelGateway id="PG_TweetRegister_Join"/> <sequenceFlow id="SF14" sourceRef="PG_TweetRegister_Join" targetRef="Event_ReceiveCourseTicket"/> <!-- Receive Course Ticket --> <intermediateCatchEvent id="Event_ReceiveCourseTicket" name="Receive Course Ticket"> <messageEventDefinition id="MED_CourseTicket"/> </intermediateCatchEvent> <endEvent id="EndEvent" name="End Registration"/> <sequenceFlow id="SF15" sourceRef="Event_ReceiveCourseTicket" targetRef="EndEvent"/> 
  </process> 
  <!-- Process: Sports Institute --> <process id="Process_Institute" isExecutable="true"> 
    <laneSet> <lane id="Lane_CourseManagement" name="Course Management"/> <lane id="Lane_AccountServices" name="Account Services"/> </laneSet> <!-- Course Selection - Course Management Lane --> <intermediateCatchEvent id="Event_SelectCourseRequest_Receive2" name="Receive Course Info Request"> <messageEventDefinition id="MED_SelectCourse_Receive"/> </intermediateCatchEvent> <task id="Task_CheckCourseSlots" name="Check Course Slots"><dataOutputAssociation id="asso3"> <targetRef>DataStore_CourseDB</targetRef> </dataOutputAssociation> </task>
    
