<intermediateThrowEvent id="Event_SendCourseInfo" name="Send Course &amp; Dates"> <messageEventDefinition id="MED_SendCourseInfo"/> </intermediateThrowEvent> <sequenceFlow id="ISI1" sourceRef="Event_SelectCourseRequest_Receive" targetRef="Task_CheckCourseSlots"/> <sequenceFlow id="ISI2" sourceRef="Task_CheckCourseSlots" targetRef="Event_SendCourseInfo"/> <!-- Message flow linking: Event_SendCourseInfo → Event_SelectCourseRequest_Receive (Student) is implicit in message flows --> <!-- Account Creation & Activation - Account Services Lane --> <!-- Receive Activation Request from Student --> <intermediateCatchEvent id="Event_ReceiveActivationReq" name="Receive Activation Request"> <messageEventDefinition id="MED_Activation_Request_Receive"/> </intermediateCatchEvent> <task id="Task_CheckEligibility" name="Check Eligibility with University"/> <exclusiveGateway id="Gateway_Eligible" name="Eligible?"/> <task id="Task_ActivateAccount" name="Activate Account"/> <intermediateThrowEvent id="Event_ActivationResponse_Send" name="Send Activation Response"> <messageEventDefinition id="MED_Activation_Response_Send"/> </intermediateThrowEvent> <sequenceFlow id="ISI3" sourceRef="Event_ReceiveActivationReq" targetRef="Task_CheckEligibility"/> <sequenceFlow id="ISI4" sourceRef="Task_CheckEligibility" targetRef="Gateway_Eligible"/> <sequenceFlow id="ISI5a" sourceRef="Gateway_Eligible" targetRef="Task_ActivateAccount"> <conditionExpression xsi:type="tFormalExpression">yes</conditionExpression> </sequenceFlow> <sequenceFlow id="ISI5b" sourceRef="Gateway_Eligible" targetRef="Event_ActivationResponse_Send"> <conditionExpression xsi:type="tFormalExpression">no</conditionExpression> </sequenceFlow> <sequenceFlow id="ISI6" sourceRef="Task_ActivateAccount" targetRef="Event_ActivationResponse_Send" /> <!-- Registration Handling - Course Management Lane --> <task id="Task_ProcessRegistration" name="Process Course Registration"> <dataInputAssociation> <sourceRef>DataStore_CourseDB</sourceRef> </dataInputAssociation> </task> <task id="Task_ValidatePayment" name="Validate Payment"/> <task id="Task_IssueCourseTicket" name="Issue Course Ticket"/> <intermediateThrowEvent id="Event_ReceiveCourseTicket_Send" name="Send Course Ticket"> <messageEventDefinition id="MED_CourseTicket_Send"/> </intermediateThrowEvent> <sequenceFlow id="ISI7" sourceRef="Event_SendCourseInfo" targetRef="Task_ProcessRegistration"/> <sequenceFlow id="ISI8" sourceRef="Task_ProcessRegistration" targetRef="Task_ValidatePayment"/> <sequenceFlow id="ISI9" sourceRef="Task_ValidatePayment" targetRef="Task_IssueCourseTicket"/> <sequenceFlow id="ISI10" sourceRef="Task_IssueCourseTicket" targetRef="Event_ReceiveCourseTicket_Send"/> <!-- Message flow linking: Event_ReceiveCourseTicket_Send → Event_ReceiveCourseTicket (Student) --> 
</process> 
