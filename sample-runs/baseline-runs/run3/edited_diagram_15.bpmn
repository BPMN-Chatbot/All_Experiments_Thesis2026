<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
             xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
             xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
             id="Definitions_1"
             targetNamespace="http://example.com/bpmn">

    <bpmn:collaboration id="Collaboration_1">
       <!-- Message Flows -->
  <messageFlow id="mf1" name="Problem Parameters" sourceRef="cust_send_problem_params" targetRef="cs_start"/>
  <messageFlow id="mf2" name="Detailed Problem Report" sourceRef="cs_send_detailed_report" targetRef="sm_receive_detailed_report"/>
  <messageFlow id="mf3" name="Detailed Problem Report to RP" sourceRef="sm_send_to_rp" targetRef="rp_receive_detailed_report"/>
  <messageFlow id="mf4" name="Status Report" sourceRef="rp_send_status_report" targetRef="sm_receive_status_report"/>
  <messageFlow id="mf5" name="Restoration Request" sourceRef="sm_send_restoration_request" targetRef="rp_wait_restoration"/>
  <messageFlow id="mf6" name="Troubleshooting Report" sourceRef="rp_send_troubleshooting" targetRef="sm_receive_troubleshooting"/>
  <messageFlow id="mf7" name="Trouble Report" sourceRef="sm_send_trouble_report" targetRef="sm_receive_status_report"/>
  <messageFlow id="mf8" name="Actual Service Performance" sourceRef="sm_send_actual_perf" targetRef="cs_receive_actual_perf"/>
  <messageFlow id="mf9" name="Problem Resolution Report" sourceRef="sm_send_resolution_report" targetRef="cs_receive_resolution"/>
  <messageFlow id="mf10" name="Customer Report" sourceRef="cs_send_customer_report" targetRef="cust_receive_report"/>
  </bpmn:collaboration>
  <!-- Customer Pool -->
  <process id="CustomerProcess" isExecutable="true">
    <laneSet>
      <lane id="CustomerLane" name="Customer"/>
    </laneSet>
    <startEvent id="cust_start" name="Service Degradation Perceived"/>
    <task id="cust_list_problem_params" name="List Problem Parameters"/>
    <intermediateThrowEvent id="cust_send_problem_params" name="Send Problem Parameters">
      <messageEventDefinition id="msgDef1"/>
    </intermediateThrowEvent>
    <endEvent id="cust_end" name="Receive Service Report"/>
    <sequenceFlow id="cust_sf1" sourceRef="cust_start" targetRef="cust_list_problem_params"/>
    <sequenceFlow id="cust_sf2" sourceRef="cust_list_problem_params" targetRef="cust_send_problem_params"/>
    <intermediateCatchEvent id="cust_receive_report" name="Receive Service Report">
      <messageEventDefinition id="msgDef2"/>
    </intermediateCatchEvent>
    <sequenceFlow id="cust_sf3" sourceRef="cust_send_problem_params" targetRef="cust_receive_report"/>
    <sequenceFlow id="cust_sf4" sourceRef="cust_receive_report" targetRef="cust_end"/>
  </process>

  <!-- TELECO Pool -->
  <process id="TelecoProcess" isExecutable="true">
    <laneSet>
      <lane id="CSLane" name="Customer Service"/>
      <lane id="SMLane" name="Service Management"/>
      <lane id="RPLane" name="Resource Provisioning"/>
      <lane id="BCLane" name="Billing & Collections"/>
    </laneSet>
    <!-- Customer Service Lane -->
    <startEvent id="cs_start" name="Receive Problem Parameters">
      <messageEventDefinition id="msgDef3"/>
    </startEvent>
    <task id="cs_enter_report" name="Enter Problem Report into System T"/>
    <dataObjectReference id="d_problem_report" dataObjectRef="problem_report"/>
    <task id="cs_compare_sla" name="Compare Report to SLA"/>
    <dataObjectReference id="d_sla_data" dataObjectRef="sla_data"/>
    <task id="cs_determine_measures" name="Determine Counter Measures & Priorities"/>
    <dataObjectReference id="d_measures" dataObjectRef="measures"/>
    <task id="cs_determine_significance" name="Determine Customer Significance"/>
    <exclusiveGateway id="cs_significance_xor" name="Premium?"/>
    <callActivity id="cs_call_extra_fix" name="Extra Problem Fix Process"/>
    <exclusiveGateway id="cs_significance_affect_xor" name="Significance Affects Measures?"/>
    <task id="cs_reprioritize" name="Re-prioritize Measures"/>
    <task id="cs_create_detailed_report" name="Create Detailed Problem Report"/>
    <dataObjectReference id="d_detailed_report" dataObjectRef="detailed_report"/>
    <intermediateThrowEvent id="cs_send_detailed_report" name="Send Detailed Problem Report">
      <messageEventDefinition id="msgDef4"/>
    </intermediateThrowEvent>
    <intermediateCatchEvent id="cs_receive_actual_perf" name="Receive Actual Service Performance">
      <messageEventDefinition id="msgDef5"/>
    </intermediateCatchEvent>
    <intermediateCatchEvent id="cs_receive_resolution" name="Receive Problem Resolution Report">
      <messageEventDefinition id="msgDef6"/>
    </intermediateCatchEvent>
    <parallelGateway id="cs_parallel_end"/>
    <task id="cs_create_customer_report" name="Create Customer Report"/>
    <task id="cs_report_rebate" name="Report SLA Violation Rebate"/>
    <intermediateThrowEvent id="cs_send_customer_report" name="Send Report to Customer">
      <messageEventDefinition id="msgDef2"/>
    </intermediateThrowEvent>
    <endEvent id="cs_end" name="CS End"/>
    <dataObject id="problem_report" name="Problem Report"/>
    <dataObject id="sla_data" name="SLA Data"/>
    <dataObject id="measures" name="Counter Measures"/>
    <dataObject id="detailed_report" name="Detailed Problem Report"/>
    <dataObject id="customer_report" name="Customer Report"/>
    <dataObject id="rebate_report" name="Rebate Report"/>
    <sequenceFlow id="csf1" sourceRef="cs_start" targetRef="cs_enter_report"/>
    <sequenceFlow id="csf2" sourceRef="cs_enter_report" targetRef="cs_compare_sla"/>
    <sequenceFlow id="csf3" sourceRef="cs_compare_sla" targetRef="cs_determine_measures"/>
    <sequenceFlow id="csf4" sourceRef="cs_determine_measures" targetRef="cs_determine_significance"/>
    <sequenceFlow id="csf5" sourceRef="cs_determine_significance" targetRef="cs_significance_xor"/>
    <sequenceFlow id="csf6" sourceRef="cs_significance_xor" targetRef="cs_call_extra_fix">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[premium]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="csf7" sourceRef="cs_significance_xor" targetRef="cs_significance_affect_xor">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[not premium]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="csf8" sourceRef="cs_call_extra_fix" targetRef="cs_significance_affect_xor"/>
    <sequenceFlow id="csf9" sourceRef="cs_significance_affect_xor" targetRef="cs_reprioritize">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[significance affects measures]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="csf10" sourceRef="cs_significance_affect_xor" targetRef="cs_create_detailed_report">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[not significance affects measures]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="csf11" sourceRef="cs_reprioritize" targetRef="cs_create_detailed_report"/>
    <sequenceFlow id="csf12" sourceRef="cs_create_detailed_report" targetRef="cs_send_detailed_report"/>
    <sequenceFlow id="csf13" sourceRef="cs_receive_actual_perf" targetRef="cs_parallel_end"/>
    <sequenceFlow id="csf14" sourceRef="cs_receive_resolution" targetRef="cs_parallel_end"/>
    <sequenceFlow id="csf15" sourceRef="cs_parallel_end" targetRef="cs_create_customer_report"/>
    <sequenceFlow id="csf16" sourceRef="cs_parallel_end" targetRef="cs_report_rebate"/>
    <sequenceFlow id="csf17" sourceRef="cs_create_customer_report" targetRef="cs_send_customer_report"/>
    <sequenceFlow id="csf18" sourceRef="cs_report_rebate" targetRef="cs_end"/>
    <sequenceFlow id="csf19" sourceRef="cs_send_customer_report" targetRef="cs_end"/>
    <dataInputAssociation id="cs_dia1">
      <sourceRef>detailed_report</sourceRef>
      <targetRef>cs_create_customer_report</targetRef>
    </dataInputAssociation>
    <dataInputAssociation id="cs_dia2">
      <sourceRef>rebate_report</sourceRef>
      <targetRef>cs_report_rebate</targetRef>
    </dataInputAssociation>
  <!-- Service Management Lane -->
    <intermediateCatchEvent id="sm_receive_detailed_report" name="Receive Detailed Problem Report">
      <messageEventDefinition id="msgDef7"/>
    </intermediateCatchEvent>
    <exclusiveGateway id="sm_analyzable_xor" name="Analyzable by SM?"/>
    <task id="sm_analyze" name="Analyze Problem"/>
    <task id="sm_create_trouble_report" name="Create Trouble Report"/>
    <intermediateThrowEvent id="sm_send_trouble_report" name="Send Trouble Report">
      <messageEventDefinition id="msgDef8"/>
    </intermediateThrowEvent>
    <intermediateThrowEvent id="sm_send_to_rp" name="Send to Resource Provisioning">
      <messageEventDefinition id="msgDef9"/>
    </intermediateThrowEvent>
    <intermediateCatchEvent id="sm_receive_status_report" name="Receive Status Report">
      <messageEventDefinition id="msgDef10"/>
    </intermediateCatchEvent>
    <task id="sm_prepare_final_status" name="Prepare Final Status Report"/>
    <exclusiveGateway id="sm_countermeasures_xor" name="Determine Counter Measures"/>
    <intermediateThrowEvent id="sm_send_actual_perf" name="Send Actual Service Performance">
      <messageEventDefinition id="msgDef5"/>
    </intermediateThrowEvent>
    <task id="sm_corrective_action" name="Undertake Corrective Actions"/>
    <task id="sm_create_resolution_report" name="Create Problem Resolution Report"/>
    <intermediateThrowEvent id="sm_send_resolution_report" name="Send Problem Resolution Report">
      <messageEventDefinition id="msgDef6"/>
    </intermediateThrowEvent>
    <intermediateThrowEvent id="sm_send_restoration_request" name="Send Restoration Request">
      <messageEventDefinition id="msgDef11"/>
    </intermediateThrowEvent>
    <intermediateCatchEvent id="sm_receive_troubleshooting" name="Receive Troubleshooting Report">
      <messageEventDefinition id="msgDef12"/>
    </intermediateCatchEvent>
    <sequenceFlow id="smf1" sourceRef="sm_receive_detailed_report" targetRef="sm_analyzable_xor"/>
    <sequenceFlow id="smf2" sourceRef="sm_analyzable_xor" targetRef="sm_analyze">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[analyzable]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="smf3" sourceRef="sm_analyze" targetRef="sm_create_trouble_report"/>
    <sequenceFlow id="smf4" sourceRef="sm_create_trouble_report" targetRef="sm_send_trouble_report"/>
    <sequenceFlow id="smf5" sourceRef="sm_analyzable_xor" targetRef="sm_send_to_rp">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[not analyzable]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="smf6" sourceRef="sm_send_trouble_report" targetRef="sm_receive_status_report"/>
    <sequenceFlow id="smf7" sourceRef="sm_send_to_rp" targetRef="sm_receive_status_report"/>
    <sequenceFlow id="smf8" sourceRef="sm_receive_status_report" targetRef="sm_prepare_final_status"/>
    <sequenceFlow id="smf9" sourceRef="sm_prepare_final_status" targetRef="sm_countermeasures_xor"/>
    <sequenceFlow id="smf10" sourceRef="sm_countermeasures_xor" targetRef="sm_send_actual_perf">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[no problem]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="smf11" sourceRef="sm_countermeasures_xor" targetRef="sm_corrective_action">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[minor corrective actions]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="smf12" sourceRef="sm_corrective_action" targetRef="sm_create_resolution_report"/>
    <sequenceFlow id="smf13" sourceRef="sm_create_resolution_report" targetRef="sm_send_resolution_report"/>
    <sequenceFlow id="smf14" sourceRef="sm_countermeasures_xor" targetRef="sm_send_restoration_request">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[automatic resource restoration]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="smf15" sourceRef="sm_send_restoration_request" targetRef="sm_receive_troubleshooting"/>
    <sequenceFlow id="smf16" sourceRef="sm_receive_troubleshooting" targetRef="sm_create_resolution_report"/>
  <!-- Resource Provisioning Lane -->
    <intermediateCatchEvent id="rp_receive_detailed_report" name="Receive Detailed Problem Report">
      <messageEventDefinition id="msgDef13"/>
    </intermediateCatchEvent>
    <exclusiveGateway id="rp_problem_xor" name="Problem Detected?"/>
    <task id="rp_analyze" name="Analyze Problem"/>
    <task id="rp_create_trouble_report" name="Create Trouble Report"/>
    <task id="rp_create_normal_notification" name="Create Normal Service Notification"/>
    <parallelGateway id="rp_status_merge"/>
    <task id="rp_create_status_report" name="Create Status Report"/>
    <intermediateThrowEvent id="rp_send_status_report" name="Send Status Report">
      <messageEventDefinition id="msgDef10"/>
    </intermediateThrowEvent>
    <intermediateCatchEvent id="rp_wait_restoration" name="Wait for Restoration Request">
      <messageEventDefinition id="msgDef11"/>
    </intermediateCatchEvent>
    <intermediateCatchEvent id="rp_timer" name="Timeout">
      <timerEventDefinition>
        <timeDuration>PT48H</timeDuration>
      </timerEventDefinition>
    </intermediateCatchEvent>
    <exclusiveGateway id="rp_restoration_xor" name="Restoration Request Received?"/>
    <task id="rp_track_errors" name="Track Errors"/>
    <task id="rp_corrective_actions" name="Undertake Corrective Actions"/>
    <task id="rp_create_troubleshooting_report" name="Create Troubleshooting Report"/>
    <intermediateThrowEvent id="rp_send_troubleshooting" name="Send Troubleshooting Report">
      <messageEventDefinition id="msgDef12"/>
    </intermediateThrowEvent>
    <endEvent id="rp_end" name="RP End"/>
    <sequenceFlow id="rpf1" sourceRef="rp_receive_detailed_report" targetRef="rp_problem_xor"/>
    <sequenceFlow id="rpf2" sourceRef="rp_problem_xor" targetRef="rp_create_normal_notification">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[no problem]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="rpf3" sourceRef="rp_problem_xor" targetRef="rp_analyze">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[problem detected]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="rpf4" sourceRef="rp_analyze" targetRef="rp_create_trouble_report"/>
    <sequenceFlow id="rpf5" sourceRef="rp_create_trouble_report" targetRef="rp_status_merge"/>
    <sequenceFlow id="rpf6" sourceRef="rp_create_normal_notification" targetRef="rp_status_merge"/>
    <sequenceFlow id="rpf7" sourceRef="rp_status_merge" targetRef="rp_create_status_report"/>
    <sequenceFlow id="rpf8" sourceRef="rp_create_status_report" targetRef="rp_send_status_report"/>
    <sequenceFlow id="rpf9" sourceRef="rp_send_status_report" targetRef="rp_wait_restoration"/>
    <sequenceFlow id="rpf10" sourceRef="rp_wait_restoration" targetRef="rp_restoration_xor"/>
    <sequenceFlow id="rpf11" sourceRef="rp_timer" targetRef="rp_restoration_xor"/>
    <sequenceFlow id="rpf12" sourceRef="rp_restoration_xor" targetRef="rp_track_errors">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[restoration request received]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="rpf13" sourceRef="rp_restoration_xor" targetRef="rp_end">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[timeout]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="rpf14" sourceRef="rp_track_errors" targetRef="rp_corrective_actions"/>
    <sequenceFlow id="rpf15" sourceRef="rp_corrective_actions" targetRef="rp_create_troubleshooting_report"/>
    <sequenceFlow id="rpf16" sourceRef="rp_create_troubleshooting_report" targetRef="rp_send_troubleshooting"/>
    <sequenceFlow id="rpf17" sourceRef="rp_send_troubleshooting" targetRef="rp_end"/>
  <!-- Billing & Collections Lane -->
    <intermediateCatchEvent id="bc_receive_rebate" name="Receive SLA Violation Rebate"/>
    <task id="bc_adjust_billing" name="Adjust Billing"/>
    <endEvent id="bc_end" name="BC End"/>
    <sequenceFlow id="bcf1" sourceRef="bc_receive_rebate" targetRef="bc_adjust_billing"/>
    <sequenceFlow id="bcf2" sourceRef="bc_adjust_billing" targetRef="bc_end"/>
  </process>
<bpmndi:BPMNDiagram id="BPMNDiagram_1">
  <bpmndi:BPMNPlane id="BPMNPlane_1" bpmnElement="Collaboration_1">

    <!-- Customer Pool (single lane) -->
    <bpmndi:BPMNShape id="CustomerPool_shape" bpmnElement="CustomerProcess" isHorizontal="true">
      <dc:Bounds x="40" y="40" width="2120" height="180"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="CustomerLane_shape" bpmnElement="CustomerLane" isHorizontal="true">
      <dc:Bounds x="60" y="60" width="2100" height="160"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="cust_start_di" bpmnElement="cust_start">
      <dc:Bounds x="100" y="110" width="36" height="36"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="cust_list_problem_params_di" bpmnElement="cust_list_problem_params">
      <dc:Bounds x="180" y="100" width="100" height="56"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="cust_send_problem_params_di" bpmnElement="cust_send_problem_params">
      <dc:Bounds x="330" y="110" width="36" height="36"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="cust_receive_report_di" bpmnElement="cust_receive_report">
      <dc:Bounds x="600" y="110" width="36" height="36"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="cust_end_di" bpmnElement="cust_end">
      <dc:Bounds x="730" y="110" width="36" height="36"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNEdge id="cust_sf1_di" bpmnElement="cust_sf1">
      <di:waypoint x="136" y="128" />
      <di:waypoint x="180" y="128" />
    </bpmndi:BPMNEdge>
    <bpmndi:BPMNEdge id="cust_sf2_di" bpmnElement="cust_sf2">
      <di:waypoint x="280" y="128" />
      <di:waypoint x="330" y="128" />
    </bpmndi:BPMNEdge>
    <bpmndi:BPMNEdge id="cust_sf3_di" bpmnElement="cust_sf3">
      <di:waypoint x="366" y="128" />
      <di:waypoint x="600" y="128"/>
    </bpmndi:BPMNEdge>
    <bpmndi:BPMNEdge id="cust_sf4_di" bpmnElement="cust_sf4">
      <di:waypoint x="636" y="128"/>
      <di:waypoint x="730" y="128"/>
    </bpmndi:BPMNEdge>

    <!-- TELECO Pool + Lanes -->
    <bpmndi:BPMNShape id="TelecoPool_shape" bpmnElement="TelecoProcess" isHorizontal="true">
      <dc:Bounds x="40" y="250" width="2120" height="540"/>
    </bpmndi:BPMNShape>
    <!-- Lanes -->
    <bpmndi:BPMNShape id="CSLane_shape" bpmnElement="CSLane" isHorizontal="true">
      <dc:Bounds x="60" y="270" width="2100" height="135"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="SMLane_shape" bpmnElement="SMLane" isHorizontal="true">
      <dc:Bounds x="60" y="405" width="2100" height="135"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="RPLane_shape" bpmnElement="RPLane" isHorizontal="true">
      <dc:Bounds x="60" y="540" width="2100" height="135"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="BCLane_shape" bpmnElement="BCLane" isHorizontal="true">
      <dc:Bounds x="60" y="675" width="2100" height="115"/>
    </bpmndi:BPMNShape>

    <!-- CS Lane -->
    <bpmndi:BPMNShape id="cs_start_di" bpmnElement="cs_start">
      <dc:Bounds x="120" y="313" width="36" height="36"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="cs_enter_report_di" bpmnElement="cs_enter_report">
      <dc:Bounds x="180" y="305" width="80" height="52"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="cs_compare_sla_di" bpmnElement="cs_compare_sla">
      <dc:Bounds x="290" y="305" width="80" height="52"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="cs_determine_measures_di" bpmnElement="cs_determine_measures">
      <dc:Bounds x="400" y="305" width="80" height="52"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="cs_determine_significance_di" bpmnElement="cs_determine_significance">
      <dc:Bounds x="510" y="305" width="80" height="52"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="cs_significance_xor_di" bpmnElement="cs_significance_xor">
      <dc:Bounds x="610" y="318" width="40" height="40"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="cs_call_extra_fix_di" bpmnElement="cs_call_extra_fix">
      <dc:Bounds x="690" y="305" width="80" height="52"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="cs_significance_affect_xor_di" bpmnElement="cs_significance_affect_xor">
      <dc:Bounds x="800" y="318" width="40" height="40"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="cs_reprioritize_di" bpmnElement="cs_reprioritize">
      <dc:Bounds x="860" y="305" width="80" height="52"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="cs_create_detailed_report_di" bpmnElement="cs_create_detailed_report">
      <dc:Bounds x="970" y="305" width="90" height="52"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="cs_send_detailed_report_di" bpmnElement="cs_send_detailed_report">
      <dc:Bounds x="1080" y="313" width="36" height="36"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="cs_receive_actual_perf_di" bpmnElement="cs_receive_actual_perf">
      <dc:Bounds x="1136" y="313" width="36" height="36"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="cs_receive_resolution_di" bpmnElement="cs_receive_resolution">
      <dc:Bounds x="1192" y="313" width="36" height="36"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="cs_parallel_end_di" bpmnElement="cs_parallel_end">
      <dc:Bounds x="1240" y="322" width="30" height="30"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="cs_create_customer_report_di" bpmnElement="cs_create_customer_report">
      <dc:Bounds x="1290" y="305" width="92" height="52"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="cs_report_rebate_di" bpmnElement="cs_report_rebate">
      <dc:Bounds x="1290" y="370" width="92" height="52"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="cs_send_customer_report_di" bpmnElement="cs_send_customer_report">
      <dc:Bounds x="1410" y="313" width="36" height="36"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="cs_end_di" bpmnElement="cs_end">
      <dc:Bounds x="1470" y="313" width="36" height="36"/>
    </bpmndi:BPMNShape>

    <!-- Key sequence flows/edges for CS lane -->
    <bpmndi:BPMNEdge id="csf1_di" bpmnElement="csf1">
      <di:waypoint x="156" y="331" />
      <di:waypoint x="180" y="331" />
    </bpmndi:BPMNEdge>
    <bpmndi:BPMNEdge id="csf2_di" bpmnElement="csf2">
      <di:waypoint x="260" y="331" />
      <di:waypoint x="290" y="331" />
    </bpmndi:BPMNEdge>
    <bpmndi:BPMNEdge id="csf3_di" bpmnElement="csf3">
      <di:waypoint x="370" y="331"/>
      <di:waypoint x="400" y="331"/>
    </bpmndi:BPMNEdge>
    <bpmndi:BPMNEdge id="csf4_di" bpmnElement="csf4">
      <di:waypoint x="480" y="331" />
      <di:waypoint x="510" y="331" />
    </bpmndi:BPMNEdge>
