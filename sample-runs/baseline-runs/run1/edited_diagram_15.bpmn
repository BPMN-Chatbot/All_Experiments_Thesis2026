<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
             xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
             xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
             id="Definitions_1"
             targetNamespace="http://example.com/bpmn">

  <process id="ServiceDegradationHandling" isExecutable="true">

    <!-- Pools -->
    <participant id="CustomerPool" name="Customer"/>
    <participant id="TelecoPool" name="TELECO"/>
    <participant id="ResourceProvisioningPool" name="Resource Provisioning"/>
    <participant id="BillingPool" name="Billing & Collections"/>

    <!-- Lanes for TELECO -->
    <laneSet id="TelecoLaneSet">
      <lane id="CustomerServiceLane" name="Customer Service"/>
      <lane id="ServiceManagementLane" name="Service Management"/>
    </laneSet>

    <!-- Lanes for Resource Provisioning -->
    <laneSet id="ResourceProvisioningLaneSet">
      <lane id="ResourceProvisioningLane" name="Resource Provisioning"/>
    </laneSet>

    <!-- Lanes for Billing -->
    <laneSet id="BillingLaneSet">
      <lane id="BillingLane" name="Billing"/>
    </laneSet>

    <!-- Customer Process -->
    <startEvent id="CustomerStart" name="Detect Service Degradation"/>
    <task id="CustomerListProblem" name="List Problem Parameters"/>
    <intermediateThrowEvent id="SendProblemToTeleco" name="Send Problem Parameters">
      <messageEventDefinition id="SendProblemToTelecoDef"/>
    </intermediateThrowEvent>
    <endEvent id="CustomerEnd" name="Receive Service Report"/>

    <!-- TELECO: Customer Service -->
    <intermediateCatchEvent id="ReceiveProblemFromCustomer" name="Receive Problem Parameters">
      <messageEventDefinition id="ReceiveProblemFromCustomerDef"/>
    </intermediateCatchEvent>
    <task id="EnterProblemReport" name="Enter Problem Report into System T"/>
    <dataObjectReference id="ProblemReportData" dataObjectRef="ProblemReport"/>
    <task id="CompareToSLA" name="Compare to SLA"/>
    <dataObjectReference id="SLAData" dataObjectRef="SLA"/>
    <task id="DetermineCounterMeasures" name="Determine Counter Measures"/>
    <dataObjectReference id="CounterMeasuresData" dataObjectRef="CounterMeasures"/>
    <callActivity id="DetermineCustomerSignificance" name="Determine Customer Significance"/>
    <exclusiveGateway id="PremiumCustomerGateway" name="Premium Customer?"/>
    <callActivity id="ExtraProblemFixProcess" name="Extra Problem Fix Process"/>
    <exclusiveGateway id="SignificanceAffectsMeasuresGateway" name="Significance Affects Measures?"/>
    <task id="ReprioritizeMeasures" name="Re-prioritize Counter Measures"/>
    <task id="CreateDetailedProblemReport" name="Create Detailed Problem Report"/>
    <dataObjectReference id="DetailedProblemReportData" dataObjectRef="DetailedProblemReport"/>
    <intermediateThrowEvent id="SendDetailedReportToSM" name="Send Detailed Problem Report to Service Management">
      <messageEventDefinition id="SendDetailedReportToSMDef"/>
    </intermediateThrowEvent>

    <!-- TELECO: Service Management -->
    <intermediateCatchEvent id="ReceiveDetailedReportFromCS" name="Receive Detailed Problem Report">
      <messageEventDefinition id="ReceiveDetailedReportFromCSDef"/>
    </intermediateCatchEvent>
    <exclusiveGateway id="AnalyzableBySMGateway" name="Analyzable by Service Management?"/>
    <intermediateThrowEvent id="SendReportToRP" name="Send Detailed Problem Report to Resource Provisioning">
      <messageEventDefinition id="SendReportToRPDef"/>
    </intermediateThrowEvent>
    <task id="AnalyzeProblemSM" name="Analyze Problem"/>
    <task id="CreateTroubleReportSM" name="Create Trouble Report"/>
    <dataObjectReference id="TroubleReportSMData" dataObjectRef="TroubleReportSM"/>
    <intermediateCatchEvent id="ReceiveStatusReportFromRP" name="Receive Status Report from Resource Provisioning">
      <messageEventDefinition id="ReceiveStatusReportFromRPDef"/>
    </intermediateCatchEvent>
    <task id="PrepareFinalStatusReport" name="Prepare Final Status Report"/>
    <exclusiveGateway id="CounterMeasureDecisionGateway" name="Counter Measure Decision"/>
    <intermediateThrowEvent id="SendPerformanceToCS" name="Send Actual Service Performance to Customer Service">
      <messageEventDefinition id="SendPerformanceToCSDef"/>
    </intermediateThrowEvent>
    <task id="UndertakeCorrectiveActionsSM" name="Undertake Corrective Actions"/>
    <task id="CreateProblemResolutionReport" name="Create Problem Resolution Report"/>
    <dataObjectReference id="ProblemResolutionReportData" dataObjectRef="ProblemResolutionReport"/>
    <intermediateThrowEvent id="SendResolutionToCS" name="Send Problem Resolution Report to Customer Service">
      <messageEventDefinition id="SendResolutionToCSDef"/>
    </intermediateThrowEvent>
    <task id="CreateRestorationRequest" name="Create Request for Automatic Resource Restoration"/>
    <intermediateThrowEvent id="SendRestorationRequestToRP" name="Send Restoration Request to Resource Provisioning">
      <messageEventDefinition id="SendRestorationRequestToRPDef"/>
    </intermediateThrowEvent>
    <intermediateCatchEvent id="ReceiveTroubleShootingReportFromRP" name="Receive Trouble-Shooting Report from Resource Provisioning">
      <messageEventDefinition id="ReceiveTroubleShootingReportFromRPDef"/>
    </intermediateCatchEvent>
    <task id="CreateProblemResolutionReport2" name="Create Problem Resolution Report"/>
    <intermediateThrowEvent id="SendResolutionToCS2" name="Send Problem Resolution Report to Customer Service">
      <messageEventDefinition id="SendResolutionToCSDef2"/>
    </intermediateThrowEvent>

    <!-- TELECO: Customer Service (after SM) -->
    <intermediateCatchEvent id="ReceivePerformanceFromSM" name="Receive Actual Service Performance">
      <messageEventDefinition id="ReceivePerformanceFromSMDef"/>
    </intermediateCatchEvent>
    <intermediateCatchEvent id="ReceiveResolutionFromSM" name="Receive Problem Resolution Report">
      <messageEventDefinition id="ReceiveResolutionFromSMDef"/>
    </intermediateCatchEvent>
    <parallelGateway id="CSParallelGateway" name="Parallel Activities"/>
    <task id="CreateCustomerReport" name="Create Customer Report"/>
    <intermediateThrowEvent id="SendReportToCustomer" name="Send Report to Customer">
      <messageEventDefinition id="SendReportToCustomerDef"/>
    </intermediateThrowEvent>
    <task id="ReportSLAViolationToBilling" name="Report SLA Violation Rebate"/>
    <intermediateThrowEvent id="SendSLAViolationToBilling" name="Send SLA Violation to Billing">
      <messageEventDefinition id="SendSLAViolationToBillingDef"/>
    </intermediateThrowEvent>
    <endEvent id="CustomerServiceEnd" name="Customer Service End"/>

    <!-- Resource Provisioning -->
    <intermediateCatchEvent id="ReceiveReportFromSM" name="Receive Detailed Problem Report">
      <messageEventDefinition id="ReceiveReportFromSMDef"/>
    </intermediateCatchEvent>
    <exclusiveGateway id="ProblemDetectedGateway" name="Problem Detected?"/>
    <task id="CreateNormalExecutionNotification" name="Create Normal Service Execution Notification"/>
    <task id="AnalyzeProblemRP" name="Analyze Problem"/>
    <task id="CreateTroubleReportRP" name="Create Trouble Report"/>
    <dataObjectReference id="TroubleReportRPData" dataObjectRef="TroubleReportRP"/>
    <task id="CreateStatusReport" name="Create Status Report"/>
    <intermediateThrowEvent id="SendStatusReportToSM" name="Send Status Report to Service Management">
      <messageEventDefinition id="SendStatusReportToSMDef"/>
    </intermediateThrowEvent>
    <intermediateCatchEvent id="ReceiveRestorationRequest" name="Receive Restoration Request">
      <messageEventDefinition id="ReceiveRestorationRequestDef"/>
    </intermediateCatchEvent>
    <intermediateCatchEvent id="RestorationTimeout" name="Restoration Timeout">
      <timerEventDefinition>
        <timeDuration>PT48H</timeDuration>
      </timerEventDefinition>
    </intermediateCatchEvent>
    <exclusiveGateway id="RestorationRequestOrTimeoutGateway" name="Restoration Request or Timeout"/>
    <task id="TrackErrors" name="Track Errors"/>
    <task id="UndertakeCorrectiveActionsRP" name="Undertake Corrective Actions"/>
    <task id="CreateTroubleShootingReport" name="Create Trouble-Shooting Report"/>
    <intermediateThrowEvent id="SendTroubleShootingReportToSM" name="Send Trouble-Shooting Report to Service Management">
      <messageEventDefinition id="SendTroubleShootingReportToSMDef"/>
    </intermediateThrowEvent>
    <endEvent id="ResourceProvisioningEnd" name="Resource Provisioning End"/>

    <!-- Billing & Collections -->
    <intermediateCatchEvent id="ReceiveSLAViolation" name="Receive SLA Violation">
      <messageEventDefinition id="ReceiveSLAViolationDef"/>
    </intermediateCatchEvent>
    <task id="AdjustBilling" name="Adjust Billing"/>
    <endEvent id="BillingEnd" name="Billing End"/>

    <!-- Data Objects -->
    <dataObject id="ProblemReport" name="Problem Report"/>
    <dataObject id="SLA" name="SLA"/>
    <dataObject id="CounterMeasures" name="Counter Measures"/>
    <dataObject id="DetailedProblemReport" name="Detailed Problem Report"/>
    <dataObject id="TroubleReportSM" name="Trouble Report (SM)"/>
    <dataObject id="TroubleReportRP" name="Trouble Report (RP)"/>
    <dataObject id="ProblemResolutionReport" name="Problem Resolution Report"/>

    <!-- Data Stores -->
    <dataStoreReference id="ContractHistoryStore" dataStoreRef="ContractHistory"/>
    <dataStore id="ContractHistory" name="Contract History"/>

    <!-- Sequence Flows: Customer -->
    <sequenceFlow id="sf1" sourceRef="CustomerStart" targetRef="CustomerListProblem"/>
    <sequenceFlow id="sf2" sourceRef="CustomerListProblem" targetRef="SendProblemToTeleco"/>
    <sequenceFlow id="sf3" sourceRef="SendReportToCustomer" targetRef="CustomerEnd"/>

    <!-- Sequence Flows: TELECO Customer Service -->
    <sequenceFlow id="sf4" sourceRef="ReceiveProblemFromCustomer" targetRef="EnterProblemReport"/>
    <sequenceFlow id="sf5" sourceRef="EnterProblemReport" targetRef="CompareToSLA"/>
    <sequenceFlow id="sf6" sourceRef="CompareToSLA" targetRef="DetermineCounterMeasures"/>
    <sequenceFlow id="sf7" sourceRef="DetermineCounterMeasures" targetRef="DetermineCustomerSignificance"/>
    <sequenceFlow id="sf8" sourceRef="DetermineCustomerSignificance" targetRef="PremiumCustomerGateway"/>
    <sequenceFlow id="sf9" sourceRef="PremiumCustomerGateway" targetRef="ExtraProblemFixProcess">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[isPremium]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="sf10" sourceRef="PremiumCustomerGateway" targetRef="SignificanceAffectsMeasuresGateway">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[!isPremium]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="sf11" sourceRef="ExtraProblemFixProcess" targetRef="SignificanceAffectsMeasuresGateway"/>
    <sequenceFlow id="sf12" sourceRef="SignificanceAffectsMeasuresGateway" targetRef="ReprioritizeMeasures">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[significanceAffectsMeasures]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="sf13" sourceRef="SignificanceAffectsMeasuresGateway" targetRef="CreateDetailedProblemReport">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[!significanceAffectsMeasures]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="sf14" sourceRef="ReprioritizeMeasures" targetRef="CreateDetailedProblemReport"/>
    <sequenceFlow id="sf15" sourceRef="CreateDetailedProblemReport" targetRef="SendDetailedReportToSM"/>

    <!-- Sequence Flows: Service Management -->
    <sequenceFlow id="sf16" sourceRef="ReceiveDetailedReportFromCS" targetRef="AnalyzableBySMGateway"/>
    <sequenceFlow id="sf17" sourceRef="AnalyzableBySMGateway" targetRef="SendReportToRP">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[notAnalyzable]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="sf18" sourceRef="AnalyzableBySMGateway" targetRef="AnalyzeProblemSM">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[analyzable]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="sf19" sourceRef="AnalyzeProblemSM" targetRef="CreateTroubleReportSM"/>
    <sequenceFlow id="sf20" sourceRef="CreateTroubleReportSM" targetRef="PrepareFinalStatusReport"/>
    <sequenceFlow id="sf21" sourceRef="SendReportToRP" targetRef="ReceiveStatusReportFromRP"/>
    <sequenceFlow id="sf22" sourceRef="ReceiveStatusReportFromRP" targetRef="PrepareFinalStatusReport"/>
    <sequenceFlow id="sf23" sourceRef="PrepareFinalStatusReport" targetRef="CounterMeasureDecisionGateway"/>
    <sequenceFlow id="sf24" sourceRef="CounterMeasureDecisionGateway" targetRef="SendPerformanceToCS">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[noProblem]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="sf25" sourceRef="CounterMeasureDecisionGateway" targetRef="UndertakeCorrectiveActionsSM">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[minorCorrectiveActions]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="sf26" sourceRef="CounterMeasureDecisionGateway" targetRef="CreateRestorationRequest">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[resourceRestorationRequired]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="sf27" sourceRef="UndertakeCorrectiveActionsSM" targetRef="CreateProblemResolutionReport"/>
    <sequenceFlow id="sf28" sourceRef="CreateProblemResolutionReport" targetRef="SendResolutionToCS"/>
    <sequenceFlow id="sf29" sourceRef="CreateRestorationRequest" targetRef="SendRestorationRequestToRP"/>
    <sequenceFlow id="sf30" sourceRef="SendRestorationRequestToRP" targetRef="ReceiveTroubleShootingReportFromRP"/>
    <sequenceFlow id="sf31" sourceRef="ReceiveTroubleShootingReportFromRP" targetRef="CreateProblemResolutionReport2"/>
    <sequenceFlow id="sf32" sourceRef="CreateProblemResolutionReport2" targetRef="SendResolutionToCS2"/>

    <!-- Sequence Flows: Customer Service (after SM) -->
    <sequenceFlow id="sf33" sourceRef="SendPerformanceToCS" targetRef="ReceivePerformanceFromSM"/>
    <sequenceFlow id="sf34" sourceRef="SendResolutionToCS" targetRef="ReceiveResolutionFromSM"/>
    <sequenceFlow id="sf35" sourceRef="SendResolutionToCS2" targetRef="ReceiveResolutionFromSM"/>
    <sequenceFlow id="sf36" sourceRef="ReceivePerformanceFromSM" targetRef="CSParallelGateway"/>
    <sequenceFlow id="sf37" sourceRef="ReceiveResolutionFromSM" targetRef="CSParallelGateway"/>
    <sequenceFlow id="sf38" sourceRef="CSParallelGateway" targetRef="CreateCustomerReport"/>
    <sequenceFlow id="sf39" sourceRef="CSParallelGateway" targetRef="ReportSLAViolationToBilling"/>
    <sequenceFlow id="sf40" sourceRef="CreateCustomerReport" targetRef="SendReportToCustomer"/>
    <sequenceFlow id="sf41" sourceRef="ReportSLAViolationToBilling" targetRef="SendSLAViolationToBilling"/>
    <sequenceFlow id="sf42" sourceRef="SendReportToCustomer" targetRef="CustomerServiceEnd"/>
    <sequenceFlow id="sf43" sourceRef="SendSLAViolationToBilling" targetRef="CustomerServiceEnd"/>

    <!-- Sequence Flows: Resource Provisioning -->
    <sequenceFlow id="sf44" sourceRef="ReceiveReportFromSM" targetRef="ProblemDetectedGateway"/>
    <sequenceFlow id="sf45" sourceRef="ProblemDetectedGateway" targetRef="CreateNormalExecutionNotification">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[noProblemDetected]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="sf46" sourceRef="ProblemDetectedGateway" targetRef="AnalyzeProblemRP">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[problemDetected]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="sf47" sourceRef="CreateNormalExecutionNotification" targetRef="CreateStatusReport"/>
    <sequenceFlow id="sf48" sourceRef="AnalyzeProblemRP" targetRef="CreateTroubleReportRP"/>
    <sequenceFlow id="sf49" sourceRef="CreateTroubleReportRP" targetRef="CreateStatusReport"/>
    <sequenceFlow id="sf50" sourceRef="CreateStatusReport" targetRef="SendStatusReportToSM"/>
    <sequenceFlow id="sf51" sourceRef="SendStatusReportToSM" targetRef="RestorationRequestOrTimeoutGateway"/>
    <sequenceFlow id="sf52" sourceRef="RestorationRequestOrTimeoutGateway" targetRef="ReceiveRestorationRequest">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[restorationRequestReceived]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="sf53" sourceRef="RestorationRequestOrTimeoutGateway" targetRef="RestorationTimeout">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[timeout]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="sf54" sourceRef="ReceiveRestorationRequest" targetRef="TrackErrors"/>
    <sequenceFlow id="sf55" sourceRef="TrackErrors" targetRef="UndertakeCorrectiveActionsRP"/>
    <sequenceFlow id="sf56" sourceRef="UndertakeCorrectiveActionsRP" targetRef="CreateTroubleShootingReport"/>
    <sequenceFlow id="sf57" sourceRef="CreateTroubleShootingReport" targetRef="SendTroubleShootingReportToSM"/>
    <sequenceFlow id="sf58" sourceRef="SendTroubleShootingReportToSM" targetRef="ResourceProvisioningEnd"/>
    <sequenceFlow id="sf59" sourceRef="RestorationTimeout" targetRef="ResourceProvisioningEnd"/>

    <!-- Sequence Flows: Billing -->
    <sequenceFlow id="sf60" sourceRef="ReceiveSLAViolation" targetRef="AdjustBilling"/>
    <sequenceFlow id="sf61" sourceRef="AdjustBilling" targetRef="BillingEnd"/>

    <!-- Message Flows -->
    <messageFlow id="mf1" sourceRef="SendProblemToTeleco" targetRef="ReceiveProblemFromCustomer"/>
    <messageFlow id="mf2" sourceRef="SendDetailedReportToSM" targetRef="ReceiveDetailedReportFromCS"/>
    <messageFlow id="mf3" sourceRef="SendReportToRP" targetRef="ReceiveReportFromSM"/>
    <messageFlow id="mf4" sourceRef="SendStatusReportToSM" targetRef="ReceiveStatusReportFromRP"/>
    <messageFlow id="mf5" sourceRef="SendRestorationRequestToRP" targetRef="ReceiveRestorationRequest"/>
    <messageFlow id="mf6" sourceRef="SendTroubleShootingReportToSM" targetRef="ReceiveTroubleShootingReportFromRP"/>
    <messageFlow id="mf7" sourceRef="SendPerformanceToCS" targetRef="ReceivePerformanceFromSM"/>
    <messageFlow id="mf8" sourceRef="SendResolutionToCS" targetRef="ReceiveResolutionFromSM"/>
    <messageFlow id="mf9" sourceRef="SendResolutionToCS2" targetRef="ReceiveResolutionFromSM"/>
    <messageFlow id="mf10" sourceRef="SendReportToCustomer" targetRef="CustomerEnd"/>
    <messageFlow id="mf11" sourceRef="SendSLAViolationToBilling" targetRef="ReceiveSLAViolation"/>

    <!-- Data Associations (examples) -->
    <dataInputAssociation id="dia1">
      <sourceRef>ReceiveProblemFromCustomer</sourceRef>
      <targetRef>ProblemReportData</targetRef>
    </dataInputAssociation>
    <dataOutputAssociation id="doa1">
      <sourceRef>EnterProblemReport</sourceRef>
      <targetRef>ProblemReportData</targetRef>
    </dataOutputAssociation>
    <dataInputAssociation id="dia2">
      <sourceRef>CompareToSLA</sourceRef>
      <targetRef>SLAData</targetRef>
    </dataInputAssociation>
    <dataOutputAssociation id="doa2">
      <sourceRef>DetermineCounterMeasures</sourceRef>
      <targetRef>CounterMeasuresData</targetRef>
    </dataOutputAssociation>
    <dataInputAssociation id="dia3">
      <sourceRef>CreateDetailedProblemReport</sourceRef>
      <targetRef>DetailedProblemReportData</targetRef>
    </dataInputAssociation>
    <dataOutputAssociation id="doa3">
      <sourceRef>CreateTroubleReportSM</sourceRef>
      <targetRef>TroubleReportSMData</targetRef>
    </dataOutputAssociation>
    <dataOutputAssociation id="doa4">
      <sourceRef>CreateTroubleReportRP</sourceRef>
      <targetRef>TroubleReportRPData</targetRef>
    </dataOutputAssociation>
    <dataOutputAssociation id="doa5">
      <sourceRef>CreateProblemResolutionReport</sourceRef>
      <targetRef>ProblemResolutionReportData</targetRef>
    </dataOutputAssociation>
    <dataOutputAssociation id="doa6">
      <sourceRef>CreateProblemResolutionReport2</sourceRef>
      <targetRef>ProblemResolutionReportData</targetRef>
    </dataOutputAssociation>

  </process>

  <bpmndi:BPMNDiagram id="BPMNDiagram_1">
  <bpmndi:BPMNPlane id="BPMNPlane_1" bpmnElement="ServiceDegradationHandling">

    <!-- Pools -->
    <bpmndi:BPMNShape id="CustomerPool_di" bpmnElement="CustomerPool" isHorizontal="true">
      <dc:Bounds x="50" y="50" width="1300" height="160"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="TelecoPool_di" bpmnElement="TelecoPool" isHorizontal="true">
      <dc:Bounds x="50" y="220" width="1300" height="380"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="ResourceProvisioningPool_di" bpmnElement="ResourceProvisioningPool" isHorizontal="true">
      <dc:Bounds x="50" y="610" width="1300" height="160"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="BillingPool_di" bpmnElement="BillingPool" isHorizontal="true">
      <dc:Bounds x="50" y="780" width="1300" height="160"/>
    </bpmndi:BPMNShape>

    <!-- TELECO LANES (Customer Service, Service Management) -->
    <bpmndi:BPMNShape id="CustomerServiceLane_di" bpmnElement="CustomerServiceLane">
      <dc:Bounds x="50" y="220" width="1300" height="190"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="ServiceManagementLane_di" bpmnElement="ServiceManagementLane">
      <dc:Bounds x="50" y="410" width="1300" height="190"/>
    </bpmndi:BPMNShape>

    <!-- Resource Provisioning Lane -->
    <bpmndi:BPMNShape id="ResourceProvisioningLane_di" bpmnElement="ResourceProvisioningLane">
      <dc:Bounds x="50" y="610" width="1300" height="160"/>
    </bpmndi:BPMNShape>
    <!-- Billing Lane -->
    <bpmndi:BPMNShape id="BillingLane_di" bpmnElement="BillingLane">
      <dc:Bounds x="50" y="780" width="1300" height="160"/>
    </bpmndi:BPMNShape>

    <!-- CUSTOMER -->
    <bpmndi:BPMNShape id="CustomerStart_di" bpmnElement="CustomerStart">
      <dc:Bounds x="100" y="110" width="36" height="36"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="CustomerListProblem_di" bpmnElement="CustomerListProblem">
      <dc:Bounds x="190" y="110" width="80" height="36"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="SendProblemToTeleco_di" bpmnElement="SendProblemToTeleco">
      <dc:Bounds x="300" y="110" width="36" height="36"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="CustomerEnd_di" bpmnElement="CustomerEnd">
      <dc:Bounds x="750" y="110" width="36" height="36"/>
    </bpmndi:BPMNShape>

    <!-- TELECO: CUSTOMER SERVICE -->
    <bpmndi:BPMNShape id="ReceiveProblemFromCustomer_di" bpmnElement="ReceiveProblemFromCustomer">
      <dc:Bounds x="420" y="270" width="36" height="36"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="EnterProblemReport_di" bpmnElement="EnterProblemReport">
      <dc:Bounds x="490" y="270" width="80" height="36"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="CompareToSLA_di" bpmnElement="CompareToSLA">
      <dc:Bounds x="590" y="270" width="80" height="36"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="DetermineCounterMeasures_di" bpmnElement="DetermineCounterMeasures">
      <dc:Bounds x="690" y="270" width="80" height="36"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="DetermineCustomerSignificance_di" bpmnElement="DetermineCustomerSignificance">
      <dc:Bounds x="790" y="270" width="90" height="36"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="PremiumCustomerGateway_di" bpmnElement="PremiumCustomerGateway">
      <dc:Bounds x="900" y="278" width="40" height="40"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="ExtraProblemFixProcess_di" bpmnElement="ExtraProblemFixProcess">
      <dc:Bounds x="970" y="265" width="80" height="36"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="SignificanceAffectsMeasuresGateway_di" bpmnElement="SignificanceAffectsMeasuresGateway">
      <dc:Bounds x="1080" y="278" width="40" height="40"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="ReprioritizeMeasures_di" bpmnElement="ReprioritizeMeasures">
      <dc:Bounds x="1150" y="250" width="90" height="36"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="CreateDetailedProblemReport_di" bpmnElement="CreateDetailedProblemReport">
      <dc:Bounds x="1250" y="270" width="90" height="36"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="SendDetailedReportToSM_di" bpmnElement="SendDetailedReportToSM">
      <dc:Bounds x="1360" y="270" width="36" height="36"/>
    </bpmndi:BPMNShape>
    <!-- Data Customer Service -->
    <bpmndi:BPMNShape id="ProblemReportData_di" bpmnElement="ProblemReportData" isExpanded="false">
      <dc:Bounds x="490" y="320" width="36" height="26"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="SLAData_di" bpmnElement="SLAData" isExpanded="false">
      <dc:Bounds x="590" y="320" width="36" height="26"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="CounterMeasuresData_di" bpmnElement="CounterMeasuresData" isExpanded="false">
      <dc:Bounds x="690" y="320" width="36" height="26"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="DetailedProblemReportData_di" bpmnElement="DetailedProblemReportData" isExpanded="false">
      <dc:Bounds x="1250" y="320" width="36" height="26"/>
    </bpmndi:BPMNShape>

    <!-- TELECO: SERVICE MANAGEMENT -->
    <bpmndi:BPMNShape id="ReceiveDetailedReportFromCS_di" bpmnElement="ReceiveDetailedReportFromCS">
      <dc:Bounds x="150" y="460" width="36" height="36"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="AnalyzableBySMGateway_di" bpmnElement="AnalyzableBySMGateway">
      <dc:Bounds x="210" y="468" width="40" height="40"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="SendReportToRP_di" bpmnElement="SendReportToRP">
      <dc:Bounds x="280" y="450" width="36" height="36"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="AnalyzeProblemSM_di" bpmnElement="AnalyzeProblemSM">
      <dc:Bounds x="350" y="450" width="90" height="36"/>
