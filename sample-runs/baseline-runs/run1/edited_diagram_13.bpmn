      <conditionExpression xsi:type="tFormalExpression">incidentType == 'accident'</conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="Flow4" sourceRef="Gateway_IncidentType" targetRef="Task_ReportNearMiss">
      <conditionExpression xsi:type="tFormalExpression">incidentType == 'near_miss'</conditionExpression>
