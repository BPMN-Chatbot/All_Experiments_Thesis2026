  </bpmn:collaboration>
  
  <bpmn:process id="CustomerProcess" isExecutable="true">
    <bpmn:laneSet>
      <bpmn:lane id="CustomerLane" name="Customer"/>
    </bpmn:laneSet>
    <bpmn:startEvent id="StartEvent_Customer" name="Start"/>
    <bpmn:sequenceFlow id="Flow_Cust_Send_Request" sourceRef="StartEvent_Customer" targetRef="Send_Request"/>
    <bpmn:task id="Send_Request" name="Send Credit Request"/>
    <bpmn:sequenceFlow id="Flow_Cust_to_End" sourceRef="Send_Request" targetRef="MessageCatch_Assessment_Result"/>
    <bpmn:intermediateCatchEvent id="MessageCatch_Assessment_Result" name="Receive Assessment Result">
      <bpmn:messageEventDefinition id="MessageEventDefinition_AssessmentResult"/>
    </bpmn:intermediateCatchEvent>
    <bpmn:sequenceFlow id="Flow_CustomerEnd" sourceRef="MessageCatch_Assessment_Result" targetRef="EndEvent_Customer"/>
    <bpmn:endEvent id="EndEvent_Customer" name="End"/>
  </bpmn:process>

  <bpmn:process id="BankProcess" isExecutable="true">
      <!-- Data Object Definitions -->
  <bpmn:dataObject id="RiskResult" name="Risk Result"/>
    <bpmn:laneSet>
      <bpmn:lane id="RiskLane" name="Risk Department"/>
      <bpmn:lane id="DisbursementLane" name="Disbursement Department"/>
    </bpmn:laneSet>
    <bpmn:startEvent id="StartEvent_Bank" name="Start"/>
    <bpmn:sequenceFlow id="Flow_Bank_Start_MsgCatch" sourceRef="StartEvent_Bank" targetRef="MessageCatch_NewRequest"/>
    <bpmn:intermediateCatchEvent id="MessageCatch_NewRequest" name="Receive Credit Request">
      <bpmn:messageEventDefinition id="MessageEventDefinition_NewRequest"/>
    </bpmn:intermediateCatchEvent>
    <bpmn:sequenceFlow id="Flow_Request_to_AssessRisk" sourceRef="MessageCatch_NewRequest" targetRef="Task_AssessRisk"/>
    <bpmn:task id="Task_AssessRisk" name="Assess Risk"/>
    <dataObjectReference id="DataObject_RiskResult" dataObjectRef="RiskResult" name="Risk Result"/>
    <bpmn:sequenceFlow id="Flow_AssessRisk_Gateway" sourceRef="Task_AssessRisk" targetRef="Gateway_RiskDecision"/>
    <bpmn:exclusiveGateway id="Gateway_RiskDecision" name="Risk Threshold?"/>
    <bpmn:sequenceFlow id="Flow_HighRisk" sourceRef="Gateway_RiskDecision" targetRef="Task_AdvancedRisk" name="Above"/>
    <bpmn:task id="Task_AdvancedRisk" name="Carry Out Advanced Risk Assessment"/>
    <bpmn:sequenceFlow id="Flow_AdvancedRisk_to_AssResult" sourceRef="Task_AdvancedRisk" targetRef="Task_AssessmentResult">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">advanced</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_SimpleRisk" sourceRef="Gateway_RiskDecision" targetRef="Task_SimpleRisk" name="Below or Equal"/>
    <bpmn:task id="Task_SimpleRisk" name="Carry Out Simple Risk Assessment"/>
    <bpmn:sequenceFlow id="Flow_SimpleRisk_to_AssResult" sourceRef="Task_SimpleRisk" targetRef="Task_AssessmentResult">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">simple</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:task id="Task_AssessmentResult" name="Prepare Assessment Result"/>
    <bpmn:sequenceFlow id="Flow_AssResult_to_NotifyCust" sourceRef="Task_AssessmentResult" targetRef="MessageThrow_NotifyCustomer"/>
    <intermediateThrowEvent id="MessageThrow_NotifyCustomer" name="Send Assessment Result">
      <bpmn:messageEventDefinition id="MessageEventDefinition_NotifyCustomer"/>
    </intermediateThrowEvent>
    <bpmn:sequenceFlow id="Flow_AssResult_to_Disbursement" sourceRef="Task_AssessmentResult" targetRef="Task_OrganizeDisbursement"/>
    <bpmn:task id="Task_OrganizeDisbursement" name="Organize Disbursement"/>
    <bpmn:endEvent id="EndEvent_Bank" name="End"/>
    <bpmn:sequenceFlow id="Flow_Disbursement_to_EndBank" sourceRef="Task_OrganizeDisbursement" targetRef="EndEvent_Bank"/>
  </bpmn:process>

  
