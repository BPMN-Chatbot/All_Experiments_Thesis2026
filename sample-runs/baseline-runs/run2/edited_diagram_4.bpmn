<?xml version="1.0" encoding="UTF-8"?> <definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" id="Definitions_1" targetNamespace="http://bpmn.io/schema/bpmn"> 

  <bpmn:collaboration id="Collaboration_1">
  <bpmn:participant id="Participant_OrderFulfillment" name="Order Fulfillment" processRef="Order_Fulfillment_Process"/>
  <bpmn:participant id="Participant_Wholesaler" name="Wholesaler" processRef="Wholesaler_Process"/>

      <!-- Message flows between pools --> <messageFlow id="MF_Reorder" sourceRef="Event_Reorder" targetRef="Wholesaler_ReceiveOrder"/> <messageFlow id="MF_ProductArrived" sourceRef="Wholesaler_DeliverProduct" targetRef="Event_ProductArrived"/> <messageFlow id="MF_Penalty" sourceRef="Event_Penalty" targetRef="Wholesaler_ReceivePenalty"/> <messageFlow id="MF_ProductArrived_AfterPenalty" sourceRef="Wholesaler_DeliverProductAfterPenalty" targetRef="Event_ProductArrived_AfterPenalty"/> 
</bpmn:collaboration>

  <process id="Order_Fulfillment_Process" isExecutable="true"> 
    <!-- Lanes --> <bpmn:laneSet id="LaneSet_1">
     
      <lane id="Lane_OrderManagement"> <flowNodeRef>Task_ReadOrder</flowNodeRef> <flowNodeRef>Task_SelectProduct</flowNodeRef> <flowNodeRef>Task_SelectNextProduct</flowNodeRef> <flowNodeRef>Task_RequestCourier</flowNodeRef> </lane> 
    <lane id="Lane_Warehouse"> <flowNodeRef>Task_CheckStock</flowNodeRef> <flowNodeRef>Task_WithdrawFromWarehouse</flowNodeRef> <flowNodeRef>Task_PackProducts</flowNodeRef> <flowNodeRef>Task_ShipProducts</flowNodeRef> </lane> 
    <lane id="Lane_StockManagement"> <flowNodeRef>Task_RegisterStock</flowNodeRef> <flowNodeRef>Task_RegisterStockAfterPenalty</flowNodeRef> </lane>
    
    </bpmn:laneSet> <!-- Data Objects and Stores --> 
    <dataObject id="DataObject_DataStore_Stock"/> 
  <dataStoreReference id="DataStore_Stock" dataStoreRef="DataObject_DataStore_Stock" name="Stock Management System"/> 
  <dataObject id="DataObject_Order" /> 
  <dataObject id="DataObject_Product" /> 
  <dataObject id="DataObject_PackingList" /> 

   <!-- References to Data Objects -->
  <dataObjectReference id="ref_Order" dataObjectRef="DataObject_Order" name="Order" />
  <dataObjectReference id="ref_Product" dataObjectRef="DataObject_Product" name="Product"/>
  <dataObjectReference id="ref_PackingList" dataObjectRef="DataObject_PackingList" name="Packing List"/>
  
  <!-- Start Event --> <startEvent id="StartEvent" name="Order Received"> 
    <outgoing>Flow_ReadOrder</outgoing> </startEvent> 
    
    <!-- Read Order Task (Order Management) --> <task id="Task_ReadOrder" name="Read Order from Automatic Order Management System"> 
      <incoming>Flow_ReadOrder</incoming> <outgoing>Flow_SelectProduct</outgoing> <dataInputAssociation id="DataInput_ReadOrder"> <sourceRef>ref_Order</sourceRef>  </dataInputAssociation> </task>  
    <!-- Select Product (Order Management) --> <task id="Task_SelectProduct" name="Select Product From Order"> <incoming>Flow_SelectProduct</incoming> <outgoing>Flow_CheckStock</outgoing> <dataInputAssociation id="DataInput_SelectProduct"> <sourceRef>ref_Order</sourceRef> </dataInputAssociation> </task> 
    
    <!-- Check Stock (Warehouse) --> <task id="Task_CheckStock" name="Check Product in Stock">
      <incoming>Flow_CheckStock</incoming> <outgoing>Flow_Gateway_InStock</outgoing> <dataInputAssociation id="DataInput_CheckStock"> <sourceRef>ref_Product</sourceRef>  </dataInputAssociation> <dataInputAssociation id="DataInput_CheckStock_DS"> <sourceRef>DataStore_Stock</sourceRef> </dataInputAssociation> </task>
    
    <!-- Decision Gateway: Product in Stock? --> <exclusiveGateway id="Gateway_ProductInStock" name="Product in Stock?"> 
      <incoming>Flow_Gateway_InStock</incoming> <outgoing>Flow_InStock</outgoing> <outgoing>Flow_NotInStock</outgoing> </exclusiveGateway> 
    <!-- In Stock Path - Withdraw from Warehouse --> <task id="Task_WithdrawFromWarehouse" name="Withdraw Product from Warehouse" >
      <incoming>Flow_InStock</incoming> <outgoing>Flow_CheckComplete</outgoing>  <dataInputAssociation id="DA_WithdrawProduct"> <sourceRef>ref_Product</sourceRef> </dataInputAssociation> <dataOutputAssociation id="DO_WithdrawProduct"> <targetRef>ref_PackingList</targetRef> </dataOutputAssociation> <dataOutputAssociation id="DO_RemoveStock"> <targetRef>DataStore_Stock</targetRef> </dataOutputAssociation>  </task>
    <!-- Not in Stock Path - Reorder from Wholesaler --> <intermediateThrowEvent id="Event_Reorder" name="Reorder from Wholesaler"> <incoming>Flow_NotInStock</incoming> <outgoing>Flow_ReorderSent</outgoing> <messageEventDefinition id="ReorderMessage" /> </intermediateThrowEvent> 
    <!-- Message Catch Event: Await Arrival from Wholesaler --> <intermediateCatchEvent id="Event_ProductArrived" name="Product Arrival from Wholesaler"> <incoming>Flow_ReorderSent</incoming> <outgoing>Flow_ReorderOrTimeout</outgoing> <messageEventDefinition id="MsgProductArrived" /> </intermediateCatchEvent> 
    <!-- Timer Event: Wait for 10 days --> <intermediateCatchEvent id="Event_10DaysWait" name="Wait 10 Days"> <incoming>Flow_ToTimeout</incoming> <outgoing>Flow_DelayPenalty</outgoing> <timerEventDefinition /> </intermediateCatchEvent> 
    <!-- Event Based Gateway: Wait for Arrival or Timeout --> <eventBasedGateway id="Gateway_ArrivalOrTimeout" name="Arrival or Timeout?"> <incoming>Flow_ReorderOrTimeout</incoming> <outgoing>Flow_ToArrived</outgoing> <outgoing>Flow_ToTimeout</outgoing> </eventBasedGateway> 
    <!-- Delivery Delay Penalty to Wholesaler --> <intermediateThrowEvent id="Event_Penalty" name="Demand Delivery Delay Penalty"> <incoming>Flow_DelayPenalty</incoming> <outgoing>Flow_WaitForProduct</outgoing> <messageEventDefinition id="PenaltyMessage"/> </intermediateThrowEvent>
    <!-- Still Wait for Product After Penalty: Awaiting Arrival --> <intermediateCatchEvent id="Event_ProductArrived_AfterPenalty" name="Product Arrived After Penalty"> <incoming>Flow_WaitForProduct</incoming> <outgoing>Flow_RegisterStockAfterPenalty</outgoing> <messageEventDefinition id="MsgProductArrived_AfterPenalty"/> </intermediateCatchEvent> 
    <!-- Register Product in Stock (Stock Management) --> <task id="Task_RegisterStock" name="Register Product in Stock Management System" > <incoming>Flow_ToArrived</incoming> <outgoing>Flow_CheckComplete</outgoing> <dataOutputAssociation id="DA_RegisterStock"> <targetRef>DataStore_Stock</targetRef> </dataOutputAssociation> </task> 
    <!-- Register Product After Penalty (Stock Management) --> <task id="Task_RegisterStockAfterPenalty" name="Register Product in Stock Management System" > <incoming>Flow_RegisterStockAfterPenalty</incoming> <outgoing>Flow_CheckComplete</outgoing> <dataOutputAssociation id="DA_RegisterStockAfterPenalty"> <targetRef>DataStore_Stock</targetRef> </dataOutputAssociation> </task> 
    <!-- Gateway: Is Order Ready for Shipment? --> <exclusiveGateway id="Gateway_OrderReady" name="Order Ready for Shipment?"> <incoming>Flow_CheckComplete</incoming> <incoming>Flow_CheckComplete2</incoming> <outgoing>Flow_OrderReady_Yes</outgoing> <outgoing>Flow_OrderReady_No</outgoing> </exclusiveGateway> 
    <!-- If Not Ready: Loop to Next Product --> <task id="Task_SelectNextProduct" name="Select Next Product" > <incoming>Flow_OrderReady_No</incoming> <outgoing>Flow_CheckStock</outgoing> <dataInputAssociation id="DI_SelectNextProduct"> <sourceRef>ref_Order</sourceRef> </dataInputAssociation> </task>  
    <!-- If Ready: Parallel Packing and Courier Request --> <parallelGateway id="Gateway_ParallelPacking" name="Pack and Request Courier"> <incoming>Flow_OrderReady_Yes</incoming> <outgoing>Flow_ToPacking</outgoing> <outgoing>Flow_ToCourier</outgoing> </parallelGateway> 

  <!-- Task: Pack Products --> 
<task id="Task_PackProducts" name="Pack Products"> 
  <incoming>Flow_ToPacking</incoming> 
  <outgoing>Flow_ToParallelJoin</outgoing> 
  <dataInputAssociation id="DI_PackProducts"> 
    <sourceRef>ref_PackingList</sourceRef> 
  </dataInputAssociation> 
  <dataOutputAssociation id="DO_PackProducts"> 
    <targetRef>ref_PackingList</targetRef> 
  </dataOutputAssociation> 
</task>

<!-- Task: Request Courier --> 
<task id="Task_RequestCourier" name="Request Courier"> 
  <incoming>Flow_ToCourier</incoming> 
  <outgoing>Flow_ToParallelJoin2</outgoing> 
</task>

<!-- Parallel Join before Shipment --> 
<parallelGateway id="Gateway_JoinShipment" name="Both Ready To Ship"> 
  <incoming>Flow_ToParallelJoin</incoming> 
  <incoming>Flow_ToParallelJoin2</incoming> 
  <outgoing>Flow_ToShip</outgoing> 
</parallelGateway>

<!-- Task: Ship Products --> 
<task id="Task_ShipProducts" name="Ship Products"> 
  <incoming>Flow_ToShip</incoming> 
  <outgoing>Flow_End</outgoing> 
</task>

<!-- End Event --> 
<endEvent id="EndEvent" name="Order Shipped"> 
  <incoming>Flow_End</incoming> 
</endEvent>


    
    <!-- Sequence Flows --> <sequenceFlow id="Flow_ReadOrder" sourceRef="StartEvent" targetRef="Task_ReadOrder"/> <sequenceFlow id="Flow_SelectProduct" sourceRef="Task_ReadOrder" targetRef="Task_SelectProduct"/> <sequenceFlow id="Flow_CheckStock" sourceRef="Task_SelectProduct" targetRef="Task_CheckStock"/> <sequenceFlow id="Flow_Gateway_InStock" sourceRef="Task_CheckStock" targetRef="Gateway_ProductInStock"/> <sequenceFlow id="Flow_InStock" sourceRef="Gateway_ProductInStock" targetRef="Task_WithdrawFromWarehouse"/> <sequenceFlow id="Flow_NotInStock" sourceRef="Gateway_ProductInStock" targetRef="Event_Reorder"/> <sequenceFlow id="Flow_ReorderSent" sourceRef="Event_Reorder" targetRef="Event_ProductArrived"/> <sequenceFlow id="Flow_ReorderOrTimeout" sourceRef="Event_ProductArrived" targetRef="Gateway_ArrivalOrTimeout"/> <sequenceFlow id="Flow_ToArrived" sourceRef="Gateway_ArrivalOrTimeout" targetRef="Task_RegisterStock"/> <sequenceFlow id="Flow_ToTimeout" sourceRef="Gateway_ArrivalOrTimeout" targetRef="Event_10DaysWait"/> <sequenceFlow id="Flow_DelayPenalty" sourceRef="Event_10DaysWait" targetRef="Event_Penalty"/> <sequenceFlow id="Flow_WaitForProduct" sourceRef="Event_Penalty" targetRef="Event_ProductArrived_AfterPenalty"/> <sequenceFlow id="Flow_RegisterStockAfterPenalty" sourceRef="Event_ProductArrived_AfterPenalty" targetRef="Task_RegisterStockAfterPenalty"/> <sequenceFlow id="Flow_CheckComplete" sourceRef="Task_WithdrawFromWarehouse" targetRef="Gateway_OrderReady"/> <sequenceFlow id="Flow_CheckComplete2" sourceRef="Task_RegisterStock" targetRef="Gateway_OrderReady"/> <sequenceFlow id="Flow_CheckComplete2_alt" sourceRef="Task_RegisterStockAfterPenalty" targetRef="Gateway_OrderReady"/> <sequenceFlow id="Flow_OrderReady_Yes" sourceRef="Gateway_OrderReady" targetRef="Gateway_ParallelPacking"/> <sequenceFlow id="Flow_OrderReady_No" sourceRef="Gateway_OrderReady" targetRef="Task_SelectNextProduct"/> <sequenceFlow id="Flow_ToPacking" sourceRef="Gateway_ParallelPacking" targetRef="Task_PackProducts"/> <sequenceFlow id="Flow_ToCourier" sourceRef="Gateway_ParallelPacking" targetRef="Task_RequestCourier"/> <sequenceFlow id="Flow_ToParallelJoin" sourceRef="Task_PackProducts" targetRef="Gateway_JoinShipment"/> <sequenceFlow id="Flow_ToParallelJoin2" sourceRef="Task_RequestCourier" targetRef="Gateway_JoinShipment"/> <sequenceFlow id="Flow_ToShip" sourceRef="Gateway_JoinShipment" targetRef="Task_ShipProducts"/> <sequenceFlow id="Flow_End" sourceRef="Task_ShipProducts" targetRef="EndEvent"/> <sequenceFlow id="Flow_NextProduct" sourceRef="Task_SelectNextProduct" targetRef="Task_CheckStock"/> 
    <!-- Lane Assignments --> 
     <!-- End of Main Process --> </process> 
  
  <!-- Wholesaler Pool with message events only --> <process id="Wholesaler_Process" isExecutable="false"> <startEvent id="Wholesaler_Start" name="Start"/> <intermediateCatchEvent id="Wholesaler_ReceiveOrder" name="Receive Reorder"> <messageEventDefinition messageRef="ReorderMessage"/> </intermediateCatchEvent> <intermediateThrowEvent id="Wholesaler_DeliverProduct" name="Deliver Product"> <messageEventDefinition messageRef="MsgProductArrived"/> </intermediateThrowEvent> <intermediateCatchEvent id="Wholesaler_ReceivePenalty" name="Receive Delay Penalty"> <messageEventDefinition messageRef="PenaltyMessage"/> </intermediateCatchEvent> <intermediateThrowEvent id="Wholesaler_DeliverProductAfterPenalty" name="Deliver (After Penalty)"> <messageEventDefinition messageRef="MsgProductArrived_AfterPenalty"/> </intermediateThrowEvent> </process> 
  
