<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
             xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
             xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
             id="Definitions_1"
             targetNamespace="http://example.com/bpmn">

    <bpmn:collaboration id="Collaboration_1">
  </bpmn:collaboration>
  <!-- Customer Pool -->
  <process id="CustomerProcess" isExecutable="true">
    <laneSet>
      <lane id="CustomerLane" name="Customer"/>
    </laneSet>
    <startEvent id="CustomerStart" name="Service Degradation Perceived"/>
    <task id="PrepareProblemParameterList" name="Prepare Problem Parameter List"/>
    <endEvent id="CustomerEnd" name="Receive Service Performance/Resolution Report"/>
    <sequenceFlow id="flow1" sourceRef="CustomerStart" targetRef="PrepareProblemParameterList"/>
    <intermediateThrowEvent id="SendProblemParameters" name="Send Problem Parameters">
      <messageEventDefinition id="SendProblemParametersDef"/>
    </intermediateThrowEvent>
    <sequenceFlow id="flow2" sourceRef="PrepareProblemParameterList" targetRef="SendProblemParameters"/>
    <intermediateCatchEvent id="ReceiveServiceReport" name="Receive Service Performance/Resolution Report">
      <messageEventDefinition id="ReceiveServiceReportDef"/>
    </intermediateCatchEvent>
    <sequenceFlow id="flow3" sourceRef="SendProblemParameters" targetRef="ReceiveServiceReport"/>
    <sequenceFlow id="flow4" sourceRef="ReceiveServiceReport" targetRef="CustomerEnd"/>
  </process>

  <!-- TELECO Pool -->
  <process id="TelecoProcess" isExecutable="true">
    <laneSet>
      <lane id="CustomerServiceLane" name="Customer Service"/>
      <lane id="ServiceManagementLane" name="Service Management"/>
      <lane id="ResourceProvisioningLane" name="Resource Provisioning"/>
      <lane id="BillingLane" name="Billing & Collections"/>
    </laneSet>
    <!-- Customer Service Lane -->
    <startEvent id="TelecoStart" name="Start"/>
    <intermediateCatchEvent id="ReceiveProblemParameters" name="Receive Problem Parameters">
      <messageEventDefinition id="ReceiveProblemParametersDef"/>
    </intermediateCatchEvent>
    <task id="EnterProblemReport" name="Enter Problem Report into System T"/>
    <dataObjectReference id="ProblemReportData" dataObjectRef="ProblemReport"/>
    <task id="CompareToSLA" name="Compare to Customer SLA"/>
    <dataObjectReference id="SLAData" dataObjectRef="SLA"/>
    <task id="DetermineCounterMeasures" name="Determine Counter Measures and Priorities"/>
    <dataObjectReference id="CounterMeasuresData" dataObjectRef="CounterMeasures"/>
    <callActivity id="DetermineCustomerSignificance" name="Determine Customer Significance"/>
    <exclusiveGateway id="IsPremiumCustomer" name="Premium Customer?"/>
    <callActivity id="ExtraProblemFixProcess" name="Extra Problem Fix Process"/>
    <exclusiveGateway id="IsSignificanceAffecting" name="Significance Affects Counter Measures?"/>
    <task id="ReprioritizeCounterMeasures" name="Re-prioritize Counter Measures"/>
    <task id="CreateDetailedProblemReport" name="Create Detailed Problem Report"/>
    <dataObjectReference id="DetailedProblemReportData" dataObjectRef="DetailedProblemReport"/>
    <intermediateThrowEvent id="SendDetailedProblemReportToSM" name="Send Detailed Problem Report to Service Management">
      <messageEventDefinition id="SendDetailedProblemReportToSMDef"/>
    </intermediateThrowEvent>
    <!-- End of Customer Service Lane -->
    <!-- Service Management Lane -->
    <intermediateCatchEvent id="ReceiveDetailedProblemReport" name="Receive Detailed Problem Report">
      <messageEventDefinition id="ReceiveDetailedProblemReportDef"/>
    </intermediateCatchEvent>
    <exclusiveGateway id="SMAnalyzable" name="Analyzable by Service Management?"/>
    <task id="SMAnalyzeProblem" name="Analyze Problem"/>
    <task id="SMCreateTroubleReport" name="Create Trouble Report"/>
    <dataObjectReference id="TroubleReportData" dataObjectRef="TroubleReport"/>
    <intermediateThrowEvent id="SendDetailedProblemReportToRP" name="Send Detailed Problem Report to Resource Provisioning">
      <messageEventDefinition id="SendDetailedProblemReportToRPDef"/>
    </intermediateThrowEvent>
    <intermediateCatchEvent id="ReceiveStatusReportFromRP" name="Receive Status Report from Resource Provisioning">
      <messageEventDefinition id="ReceiveStatusReportFromRPDef"/>
    </intermediateCatchEvent>
    <task id="PrepareFinalStatusReport" name="Prepare Final Status Report"/>
    <exclusiveGateway id="DetermineCounterMeasure" name="Determine Counter Measure"/>
    <task id="SendActualServicePerformance" name="Send Actual Service Performance to Customer Service"/>
    <intermediateThrowEvent id="SendActualServicePerformanceEvent" name="Send Actual Service Performance">
      <messageEventDefinition id="SendActualServicePerformanceDef"/>
    </intermediateThrowEvent>
    <task id="UndertakeCorrectiveActions" name="Undertake Corrective Actions"/>
    <task id="CreateProblemResolutionReport" name="Create Problem Resolution Report"/>
    <dataObjectReference id="ProblemResolutionReportData" dataObjectRef="ProblemResolutionReport"/>
    <intermediateThrowEvent id="SendProblemResolutionReport" name="Send Problem Resolution Report to Customer Service">
      <messageEventDefinition id="SendProblemResolutionReportDef"/>
    </intermediateThrowEvent>
    <task id="CreateRestorationRequest" name="Create Request for Automatic Resource Restoration"/>
    <intermediateThrowEvent id="SendRestorationRequestToRP" name="Send Restoration Request to Resource Provisioning">
      <messageEventDefinition id="SendRestorationRequestToRPDef"/>
    </intermediateThrowEvent>
    <intermediateCatchEvent id="ReceiveTroubleshootingReport" name="Receive Trouble-shooting Report from Resource Provisioning">
      <messageEventDefinition id="ReceiveTroubleshootingReportDef"/>
    </intermediateCatchEvent>
    <endEvent id="ServiceManagementEnd" name="End"/>
    <!-- End of Service Management Lane -->
    <!-- Resource Provisioning Lane -->
    <intermediateCatchEvent id="ReceiveDetailedProblemReportFromSM" name="Receive Detailed Problem Report from Service Management">
      <messageEventDefinition id="ReceiveDetailedProblemReportFromSMDef"/>
    </intermediateCatchEvent>
    <exclusiveGateway id="RPProblemDetected" name="Problem Detected?"/>
    <task id="CreateNormalExecutionNotification" name="Create Normal Service Execution Notification"/>
    <task id="RPAnalyzeProblem" name="Analyze Problem"/>
    <task id="RPCreateTroubleReport" name="Create Trouble Report"/>
    <task id="RPCreateStatusReport" name="Create Status Report"/>
    <intermediateThrowEvent id="SendStatusReportToSM" name="Send Status Report to Service Management">
      <messageEventDefinition id="SendStatusReportToSMDef"/>
    </intermediateThrowEvent>
    <intermediateCatchEvent id="ReceiveRestorationRequest" name="Receive Restoration Request">
      <messageEventDefinition id="ReceiveRestorationRequestDef"/>
    </intermediateCatchEvent>
    <intermediateCatchEvent id="RestorationTimeout" name="Restoration Request Timeout">
      <timerEventDefinition>
        <timeDuration>PT48H</timeDuration>
      </timerEventDefinition>
    </intermediateCatchEvent>
    <exclusiveGateway id="RestorationRequestArrived" name="Restoration Request Arrived?"/>
    <task id="TrackErrors" name="Track Errors"/>
    <task id="RPCorrectiveActions" name="Undertake Corrective Actions"/>
    <task id="RPCreateTroubleshootingReport" name="Create Trouble-shooting Report"/>
    <intermediateThrowEvent id="SendTroubleshootingReportToSM" name="Send Trouble-shooting Report to Service Management">
      <messageEventDefinition id="SendTroubleshootingReportToSMDef"/>
    </intermediateThrowEvent>
    <endEvent id="ResourceProvisioningEnd" name="End"/>
    <!-- End of Resource Provisioning Lane -->
    <!-- Billing & Collections Lane -->
    <intermediateCatchEvent id="ReceiveSLAViolationRebate" name="Receive SLA Violation Rebate"/>
    <task id="AdjustBilling" name="Adjust Billing"/>
    <endEvent id="BillingEnd" name="End"/>
    <!-- End of Billing & Collections Lane -->
    <!-- Data Objects -->
    <dataObject id="ProblemReport" name="Problem Report"/>
    <dataObject id="SLA" name="Customer SLA"/>
    <dataObject id="CounterMeasures" name="Counter Measures"/>
    <dataObject id="DetailedProblemReport" name="Detailed Problem Report"/>
    <dataObject id="TroubleReport" name="Trouble Report"/>
    <dataObject id="ProblemResolutionReport" name="Problem Resolution Report"/>
    <!-- Sequence Flows: Customer Service Lane -->
    <sequenceFlow id="t1" sourceRef="TelecoStart" targetRef="ReceiveProblemParameters"/>
    <sequenceFlow id="t2" sourceRef="ReceiveProblemParameters" targetRef="EnterProblemReport"/>
    <sequenceFlow id="t3" sourceRef="EnterProblemReport" targetRef="CompareToSLA"/>
    <dataInputAssociation id="d1">
      <sourceRef>EnterProblemReport</sourceRef>
      <targetRef>ProblemReportData</targetRef>
    </dataInputAssociation>
    <sequenceFlow id="t4" sourceRef="CompareToSLA" targetRef="DetermineCounterMeasures"/>
    <dataInputAssociation id="d2">
      <sourceRef>CompareToSLA</sourceRef>
      <targetRef>SLAData</targetRef>
    </dataInputAssociation>
    <sequenceFlow id="t5" sourceRef="DetermineCounterMeasures" targetRef="DetermineCustomerSignificance"/>
    <dataInputAssociation id="d3">
      <sourceRef>DetermineCounterMeasures</sourceRef>
      <targetRef>CounterMeasuresData</targetRef>
    </dataInputAssociation>
    <sequenceFlow id="t6" sourceRef="DetermineCustomerSignificance" targetRef="IsPremiumCustomer"/>
    <sequenceFlow id="t7" sourceRef="IsPremiumCustomer" targetRef="ExtraProblemFixProcess">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[isPremium]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="t8" sourceRef="IsPremiumCustomer" targetRef="IsSignificanceAffecting">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[!isPremium]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="t9" sourceRef="ExtraProblemFixProcess" targetRef="CreateDetailedProblemReport"/>
    <sequenceFlow id="t10" sourceRef="IsSignificanceAffecting" targetRef="ReprioritizeCounterMeasures">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[significanceAffects]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="t11" sourceRef="IsSignificanceAffecting" targetRef="CreateDetailedProblemReport">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[!significanceAffects]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="t12" sourceRef="ReprioritizeCounterMeasures" targetRef="CreateDetailedProblemReport"/>
    <sequenceFlow id="t13" sourceRef="CreateDetailedProblemReport" targetRef="SendDetailedProblemReportToSM"/>
    <dataInputAssociation id="d4">
      <sourceRef>CreateDetailedProblemReport</sourceRef>
      <targetRef>DetailedProblemReportData</targetRef>
    </dataInputAssociation>
    <!-- Sequence Flows: Service Management Lane -->
    <sequenceFlow id="t14" sourceRef="SendDetailedProblemReportToSM" targetRef="ReceiveDetailedProblemReport"/>
    <sequenceFlow id="t15" sourceRef="ReceiveDetailedProblemReport" targetRef="SMAnalyzable"/>
    <sequenceFlow id="t16" sourceRef="SMAnalyzable" targetRef="SMAnalyzeProblem">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[analyzable]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="t17" sourceRef="SMAnalyzable" targetRef="SendDetailedProblemReportToRP">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[!analyzable]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="t18" sourceRef="SMAnalyzeProblem" targetRef="SMCreateTroubleReport"/>
    <sequenceFlow id="t19" sourceRef="SMCreateTroubleReport" targetRef="PrepareFinalStatusReport"/>
    <dataInputAssociation id="d5">
      <sourceRef>SMCreateTroubleReport</sourceRef>
      <targetRef>TroubleReportData</targetRef>
    </dataInputAssociation>
    <sequenceFlow id="t20" sourceRef="SendDetailedProblemReportToRP" targetRef="ReceiveStatusReportFromRP"/>
    <sequenceFlow id="t21" sourceRef="ReceiveStatusReportFromRP" targetRef="PrepareFinalStatusReport"/>
    <sequenceFlow id="t22" sourceRef="PrepareFinalStatusReport" targetRef="DetermineCounterMeasure"/>
    <sequenceFlow id="t23" sourceRef="DetermineCounterMeasure" targetRef="SendActualServicePerformance">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[noProblem]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="t24" sourceRef="DetermineCounterMeasure" targetRef="UndertakeCorrectiveActions">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[minorCorrective]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="t25" sourceRef="DetermineCounterMeasure" targetRef="CreateRestorationRequest">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[autoRestoration]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="t26" sourceRef="SendActualServicePerformance" targetRef="SendActualServicePerformanceEvent"/>
    <sequenceFlow id="t27" sourceRef="SendActualServicePerformanceEvent" targetRef="ServiceManagementEnd"/>
    <sequenceFlow id="t28" sourceRef="UndertakeCorrectiveActions" targetRef="CreateProblemResolutionReport"/>
    <sequenceFlow id="t29" sourceRef="CreateProblemResolutionReport" targetRef="SendProblemResolutionReport"/>
    <dataInputAssociation id="d6">
      <sourceRef>CreateProblemResolutionReport</sourceRef>
      <targetRef>ProblemResolutionReportData</targetRef>
    </dataInputAssociation>
    <sequenceFlow id="t30" sourceRef="SendProblemResolutionReport" targetRef="ServiceManagementEnd"/>
    <sequenceFlow id="t31" sourceRef="CreateRestorationRequest" targetRef="SendRestorationRequestToRP"/>
    <sequenceFlow id="t32" sourceRef="SendRestorationRequestToRP" targetRef="ReceiveTroubleshootingReport"/>
    <sequenceFlow id="t33" sourceRef="ReceiveTroubleshootingReport" targetRef="CreateProblemResolutionReport"/>
    <!-- Sequence Flows: Resource Provisioning Lane -->
    <sequenceFlow id="t34" sourceRef="SendDetailedProblemReportToRP" targetRef="ReceiveDetailedProblemReportFromSM"/>
    <sequenceFlow id="t35" sourceRef="ReceiveDetailedProblemReportFromSM" targetRef="RPProblemDetected"/>
    <sequenceFlow id="t36" sourceRef="RPProblemDetected" targetRef="CreateNormalExecutionNotification">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[!problemDetected]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="t37" sourceRef="RPProblemDetected" targetRef="RPAnalyzeProblem">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[problemDetected]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="t38" sourceRef="CreateNormalExecutionNotification" targetRef="RPCreateStatusReport"/>
    <sequenceFlow id="t39" sourceRef="RPAnalyzeProblem" targetRef="RPCreateTroubleReport"/>
    <sequenceFlow id="t40" sourceRef="RPCreateTroubleReport" targetRef="RPCreateStatusReport"/>
    <sequenceFlow id="t41" sourceRef="RPCreateStatusReport" targetRef="SendStatusReportToSM"/>
    <sequenceFlow id="t42" sourceRef="SendStatusReportToSM" targetRef="RestorationRequestArrived"/>
    <sequenceFlow id="t43" sourceRef="RestorationRequestArrived" targetRef="ReceiveRestorationRequest">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[restorationRequest]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="t44" sourceRef="RestorationRequestArrived" targetRef="RestorationTimeout">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[!restorationRequest]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="t45" sourceRef="ReceiveRestorationRequest" targetRef="TrackErrors"/>
    <sequenceFlow id="t46" sourceRef="TrackErrors" targetRef="RPCorrectiveActions"/>
    <sequenceFlow id="t47" sourceRef="RPCorrectiveActions" targetRef="RPCreateTroubleshootingReport"/>
    <sequenceFlow id="t48" sourceRef="RPCreateTroubleshootingReport" targetRef="SendTroubleshootingReportToSM"/>
    <sequenceFlow id="t49" sourceRef="SendTroubleshootingReportToSM" targetRef="ResourceProvisioningEnd"/>
    <sequenceFlow id="t50" sourceRef="RestorationTimeout" targetRef="ResourceProvisioningEnd"/>
    <!-- Sequence Flows: Billing & Collections Lane -->
    <sequenceFlow id="t51" sourceRef="ReceiveSLAViolationRebate" targetRef="AdjustBilling"/>
    <sequenceFlow id="t52" sourceRef="AdjustBilling" targetRef="BillingEnd"/>
    <!-- Data Flows between Lanes (Data Associations) -->
    <!-- Customer Service: After receiving actual service performance or problem resolution report, parallel activities -->
    <parallelGateway id="CSParallelGatewayStart" name="Start Parallel Activities"/>
    <parallelGateway id="CSParallelGatewayEnd" name="End Parallel Activities"/>
    <intermediateCatchEvent id="ReceiveActualServicePerformance" name="Receive Actual Service Performance">
      <messageEventDefinition id="ReceiveActualServicePerformanceDef"/>
    </intermediateCatchEvent>
    <intermediateCatchEvent id="ReceiveProblemResolutionReport" name="Receive Problem Resolution Report">
      <messageEventDefinition id="ReceiveProblemResolutionReportDef"/>
    </intermediateCatchEvent>
    <task id="CreateCustomerReport" name="Create Report for Customer"/>
    <task id="ReportSLAViolationRebate" name="Report SLA Violation Rebate"/>
    <intermediateThrowEvent id="SendCustomerReport" name="Send Report to Customer">
      <messageEventDefinition id="SendCustomerReportDef"/>
    </intermediateThrowEvent>
    <sequenceFlow id="t53" sourceRef="ReceiveActualServicePerformance" targetRef="CSParallelGatewayStart"/>
    <sequenceFlow id="t54" sourceRef="ReceiveProblemResolutionReport" targetRef="CSParallelGatewayStart"/>
    <sequenceFlow id="t55" sourceRef="CSParallelGatewayStart" targetRef="CreateCustomerReport"/>
    <sequenceFlow id="t56" sourceRef="CSParallelGatewayStart" targetRef="ReportSLAViolationRebate"/>
    <sequenceFlow id="t57" sourceRef="CreateCustomerReport" targetRef="SendCustomerReport"/>
    <sequenceFlow id="t58" sourceRef="SendCustomerReport" targetRef="CSParallelGatewayEnd"/>
    <sequenceFlow id="t59" sourceRef="ReportSLAViolationRebate" targetRef="ReceiveSLAViolationRebate"/>
    <sequenceFlow id="t60" sourceRef="CSParallelGatewayEnd" targetRef="BillingEnd"/>
    <endEvent id="CustomerServiceEnd" name="End"/>
    <sequenceFlow id="t61" sourceRef="CSParallelGatewayEnd" targetRef="CustomerServiceEnd"/>
  </process>

  <!-- Message Flows between Pools -->
  <messageFlow id="mf1" sourceRef="SendProblemParameters" targetRef="ReceiveProblemParameters"/>
  <messageFlow id="mf2" sourceRef="SendDetailedProblemReportToSM" targetRef="ReceiveDetailedProblemReport"/>
  <messageFlow id="mf3" sourceRef="SendDetailedProblemReportToRP" targetRef="ReceiveDetailedProblemReportFromSM"/>
  <messageFlow id="mf4" sourceRef="SendStatusReportToSM" targetRef="ReceiveStatusReportFromRP"/>
  <messageFlow id="mf5" sourceRef="SendActualServicePerformanceEvent" targetRef="ReceiveActualServicePerformance"/>
  <messageFlow id="mf6" sourceRef="SendProblemResolutionReport" targetRef="ReceiveProblemResolutionReport"/>
  <messageFlow id="mf7" sourceRef="SendRestorationRequestToRP" targetRef="ReceiveRestorationRequest"/>
  <messageFlow id="mf8" sourceRef="SendTroubleshootingReportToSM" targetRef="ReceiveTroubleshootingReport"/>
  <messageFlow id="mf9" sourceRef="SendCustomerReport" targetRef="ReceiveServiceReport"/>

  <bpmndi:BPMNDiagram id="BPMNDiagram_1">
  <bpmndi:BPMNPlane id="BPMNPlane_1" bpmnElement="Collaboration_1">

    <!-- Customer Pool -->
    <bpmndi:BPMNShape id="CustomerPool_di" bpmnElement="CustomerProcess" isHorizontal="true">
      <dc:Bounds x="60" y="60" width="1920" height="240"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="CustomerLane_di" bpmnElement="CustomerLane">
      <dc:Bounds x="60" y="60" width="1920" height="240"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="CustomerStart_di" bpmnElement="CustomerStart">
      <dc:Bounds x="100" y="150" width="36" height="36"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="PrepareProblemParameterList_di" bpmnElement="PrepareProblemParameterList">
      <dc:Bounds x="220" y="140" width="100" height="56"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="SendProblemParameters_di" bpmnElement="SendProblemParameters">
      <dc:Bounds x="370" y="150" width="36" height="36"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="ReceiveServiceReport_di" bpmnElement="ReceiveServiceReport">
      <dc:Bounds x="600" y="150" width="36" height="36"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="CustomerEnd_di" bpmnElement="CustomerEnd">
      <dc:Bounds x="720" y="150" width="36" height="36"/>
    </bpmndi:BPMNShape>

    <bpmndi:BPMNEdge id="flow1_di" bpmnElement="flow1">
      <di:waypoint x="136" y="168"/>
      <di:waypoint x="220" y="168"/>
    </bpmndi:BPMNEdge>
    <bpmndi:BPMNEdge id="flow2_di" bpmnElement="flow2">
      <di:waypoint x="320" y="168"/>
      <di:waypoint x="370" y="168"/>
    </bpmndi:BPMNEdge>
    <bpmndi:BPMNEdge id="flow3_di" bpmnElement="flow3">
      <di:waypoint x="406" y="168"/>
      <di:waypoint x="600" y="168"/>
    </bpmndi:BPMNEdge>
    <bpmndi:BPMNEdge id="flow4_di" bpmnElement="flow4">
      <di:waypoint x="636" y="168"/>
      <di:waypoint x="720" y="168"/>
    </bpmndi:BPMNEdge>

    <!-- Teleco Pool -->
    <bpmndi:BPMNShape id="TelecoPool_di" bpmnElement="TelecoProcess" isHorizontal="true">
      <dc:Bounds x="60" y="340" width="2600" height="620"/>
    </bpmndi:BPMNShape>
    <!-- Lanes -->
    <bpmndi:BPMNShape id="CustomerServiceLane_di" bpmnElement="CustomerServiceLane">
      <dc:Bounds x="60" y="340" width="2600" height="155"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="ServiceManagementLane_di" bpmnElement="ServiceManagementLane">
      <dc:Bounds x="60" y="495" width="2600" height="155"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="ResourceProvisioningLane_di" bpmnElement="ResourceProvisioningLane">
      <dc:Bounds x="60" y="650" width="2600" height="155"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="BillingLane_di" bpmnElement="BillingLane">
      <dc:Bounds x="60" y="805" width="2600" height="155"/>
    </bpmndi:BPMNShape>

    <!-- --- Customer Service Lane --- -->
    <bpmndi:BPMNShape id="TelecoStart_di" bpmnElement="TelecoStart">
      <dc:Bounds x="100" y="390" width="36" height="36"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="ReceiveProblemParameters_di" bpmnElement="ReceiveProblemParameters">
      <dc:Bounds x="180" y="390" width="36" height="36"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="EnterProblemReport_di" bpmnElement="EnterProblemReport">
      <dc:Bounds x="260" y="380" width="100" height="56"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="ProblemReportData_di" bpmnElement="ProblemReportData">
      <dc:Bounds x="265" y="441" width="36" height="36"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="CompareToSLA_di" bpmnElement="CompareToSLA">
      <dc:Bounds x="390" y="380" width="120" height="56"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="SLAData_di" bpmnElement="SLAData">
      <dc:Bounds x="440" y="441" width="36" height="36"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="DetermineCounterMeasures_di" bpmnElement="DetermineCounterMeasures">
      <dc:Bounds x="550" y="380" width="130" height="56"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="CounterMeasuresData_di" bpmnElement="CounterMeasuresData">
      <dc:Bounds x="600" y="441" width="36" height="36"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="DetermineCustomerSignificance_di" bpmnElement="DetermineCustomerSignificance">
      <dc:Bounds x="715" y="380" width="110" height="56"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="IsPremiumCustomer_di" bpmnElement="IsPremiumCustomer">
      <dc:Bounds x="850" y="394" width="40" height="40"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="ExtraProblemFixProcess_di" bpmnElement="ExtraProblemFixProcess">
      <dc:Bounds x="940" y="350" width="120" height="56"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="IsSignificanceAffecting_di" bpmnElement="IsSignificanceAffecting">
      <dc:Bounds x="940" y="430" width="40" height="40"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="ReprioritizeCounterMeasures_di" bpmnElement="ReprioritizeCounterMeasures">
      <dc:Bounds x="1040" y="425" width="130" height="56"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="CreateDetailedProblemReport_di" bpmnElement="CreateDetailedProblemReport">
      <dc:Bounds x="1200" y="390" width="120" height="56"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="DetailedProblemReportData_di" bpmnElement="DetailedProblemReportData">
      <dc:Bounds x="1210" y="451" width="36" height="36"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="SendDetailedProblemReportToSM_di" bpmnElement="SendDetailedProblemReportToSM">
      <dc:Bounds x="1345" y="405" width="36" height="36"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="CSParallelGatewayStart_di" bpmnElement="CSParallelGatewayStart">
      <dc:Bounds x="1720" y="415" width="40" height="40"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="CSParallelGatewayEnd_di" bpmnElement="CSParallelGatewayEnd">
      <dc:Bounds x="2010" y="415" width="40" height="40"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="ReceiveActualServicePerformance_di" bpmnElement="ReceiveActualServicePerformance">
      <dc:Bounds x="1650" y="370" width="36" height="36"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="ReceiveProblemResolutionReport_di" bpmnElement="ReceiveProblemResolutionReport">
      <dc:Bounds x="1650" y="470" width="36" height="36"/>
    </bpmndi:BPMNShape>
    <bpmndi:BPMNShape id="CreateCustomerReport_di" bpmnElement="CreateCustomerReport">
