<?xml version="1.0" encoding="UTF-8"?> <definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" id="Definitions_1" targetNamespace="http://example.com/bpmn2"> <process id="MotorClaimHandlingProcess" isExecutable="true"> <!-- Data Stores and Objects --> <dataObjectReference id="ClaimDocuments" dataObjectRef="ClaimDocuments_DO" name="Claim Documents"/> <dataObject id="ClaimDocuments_DO" /> <dataObjectReference id="ClaimInformation" dataObjectRef="ClaimInformation_DO" name="Claim Information"/> <dataObject id="ClaimInformation_DO" /> <!-- Pools & Lanes --> <laneSet id="LaneSet_1"> <lane id="Lane_Customer" name="Customer"> <flowNodeRef>StartEvent_Customer</flowNodeRef> <flowNodeRef>SubmitClaimTask</flowNodeRef> <flowNodeRef>MessageThrowEvent_Submit</flowNodeRef> <flowNodeRef>MessageCatchEvent_Letter</flowNodeRef> <flowNodeRef>EndEvent_Customer</flowNodeRef> </lane> <lane id="Lane_NotificationDepartment" name="Notification Department"> <flowNodeRef>MessageCatchEvent_ReceiveClaim</flowNodeRef> <flowNodeRef>CheckDocumentsTask</flowNodeRef> <flowNodeRef>RegisterClaimTask</flowNodeRef> </lane> <lane id="Lane_HandlingDepartment" name="Handling Department"> <flowNodeRef>CheckInsuranceTask</flowNodeRef> <flowNodeRef>AssessmentTask</flowNodeRef> <flowNodeRef>ExclusiveGateway_AssessmentResult</flowNodeRef> <flowNodeRef>RejectClaimTask</flowNodeRef> <flowNodeRef>AuthorizeRepairsTask</flowNodeRef> <flowNodeRef>SchedulePaymentTask</flowNodeRef> <flowNodeRef>SendLetterTask</flowNodeRef> <flowNodeRef>MessageThrowEvent_Letter</flowNodeRef> <flowNodeRef>EndEvent_Insurer</flowNodeRef> </lane> </laneSet> <!-- Flow objects and event definitions --> <!-- Customer submits claim --> <startEvent id="StartEvent_Customer" name="Start"> <outgoing>Flow_Cust_SubmitClaim</outgoing> </startEvent> <task id="SubmitClaimTask" name="Submit Claim"> <incoming>Flow_Cust_SubmitClaim</incoming> <outgoing>Flow_Cust_ThrowMsg</outgoing> <dataOutputAssociation id="asso1"> <targetRef>ClaimDocuments</targetRef> </dataOutputAssociation> </task> <sequenceFlow id="Flow_Cust_SubmitClaim" sourceRef="StartEvent_Customer" targetRef="SubmitClaimTask"/> <intermediateThrowEvent id="MessageThrowEvent_Submit" name="Send Claim"> <incoming>Flow_Cust_ThrowMsg</incoming> <outgoing>Flow_Cust_EndToInsurer</outgoing> <messageEventDefinition id="MessageEventDef_SendClaim"/> </intermediateThrowEvent> <sequenceFlow id="Flow_Cust_ThrowMsg" sourceRef="SubmitClaimTask" targetRef="MessageThrowEvent_Submit"/> <!-- Insurer receives claim (Notification Department) --> <intermediateCatchEvent id="MessageCatchEvent_ReceiveClaim" name="Receive Claim"> <incoming>Flow_Cust_EndToInsurer</incoming> <outgoing>Flow_CheckDocuments</outgoing> <messageEventDefinition id="MessageEventDef_ReceiveClaim"/> </intermediateCatchEvent> <!-- Message Flow: Customer -> Insurer (Notification Department) --> <sequenceFlow id="Flow_Cust_EndToInsurer" sourceRef="MessageThrowEvent_Submit" targetRef="MessageCatchEvent_ReceiveClaim"/> <task id="CheckDocumentsTask" name="Check Documents"> <incoming>Flow_CheckDocuments</incoming> <outgoing>Flow_CheckDocuments_Exclusive</outgoing> <dataInputAssociation id="asso2"> <sourceRef>ClaimDocuments</sourceRef> </dataInputAssociation> </task> <sequenceFlow id="Flow_CheckDocuments" sourceRef="MessageCatchEvent_ReceiveClaim" targetRef="CheckDocumentsTask"/> <!-- Exclusive Gateways for Document Check --> <exclusiveGateway id="ExclusiveGateway_CheckDocuments" name="Documents Complete?"> <incoming>Flow_CheckDocuments_Exclusive</incoming> <outgoing>Flow_DocsOK_RegisterClaim</outgoing> <outgoing>Flow_DocsNotOK_RejectClaim</outgoing> </exclusiveGateway> <sequenceFlow id="Flow_CheckDocuments_Exclusive" sourceRef="CheckDocumentsTask" targetRef="ExclusiveGateway_CheckDocuments"/> <!-- If complete, register claim --> <task id="RegisterClaimTask" name="Register Claim"> <incoming>Flow_DocsOK_RegisterClaim</incoming> <outgoing>Flow_RegisterClaim_Handling</outgoing> <dataOutputAssociation id="asso3"> <targetRef>ClaimInformation</targetRef> </dataOutputAssociation> </task> <sequenceFlow id="Flow_DocsOK_RegisterClaim" sourceRef="ExclusiveGateway_CheckDocuments" targetRef="RegisterClaimTask"/> <!-- If not, reject claim --> <task id="RejectClaimTask" name="Reject Claim"> <incoming>Flow_DocsNotOK_RejectClaim</incoming> <incoming>Flow_CheckIns_NOK</incoming> <incoming>Flow_Assessment_NOK</incoming> <outgoing>Flow_Reject_Letter</outgoing> </task> <sequenceFlow id="Flow_DocsNotOK_RejectClaim" sourceRef="ExclusiveGateway_CheckDocuments" targetRef="RejectClaimTask"/> <!-- Next: Handling Department picks up claim --> <task id="CheckInsuranceTask" name="Check Insurance"> <incoming>Flow_RegisterClaim_Handling</incoming> <outgoing>Flow_CheckIns_Exclusive</outgoing> <dataInputAssociation id="asso4"> <sourceRef>ClaimInformation</sourceRef> </dataInputAssociation> </task> <sequenceFlow id="Flow_RegisterClaim_Handling" sourceRef="RegisterClaimTask" targetRef="CheckInsuranceTask"/> <!-- Exclusive Gateway for Insurance Check --> <exclusiveGateway id="ExclusiveGateway_CheckInsurance" name="Insurance OK?"> <incoming>Flow_CheckIns_Exclusive</incoming> <outgoing>Flow_CheckIns_OK_Assess</outgoing> <outgoing>Flow_CheckIns_NOK</outgoing> </exclusiveGateway> <sequenceFlow id="Flow_CheckIns_Exclusive" sourceRef="CheckInsuranceTask" targetRef="ExclusiveGateway_CheckInsurance"/> <sequenceFlow id="Flow_CheckIns_OK_Assess" sourceRef="ExclusiveGateway_CheckInsurance" targetRef="AssessmentTask"/> <sequenceFlow id="Flow_CheckIns_NOK" sourceRef="ExclusiveGateway_CheckInsurance" targetRef="RejectClaimTask"/> <!-- Assessment --> <task id="AssessmentTask" name="Perform Assessment"> <incoming>Flow_CheckIns_OK_Assess</incoming> <outgoing>Flow_Assessment_Exclusive</outgoing> </task> <sequenceFlow id="Flow_Assessment_Exclusive" sourceRef="AssessmentTask" targetRef="ExclusiveGateway_AssessmentResult"/> <!-- Exclusive Gateway Assessment --> <exclusiveGateway id="ExclusiveGateway_AssessmentResult" name="Assessment Positive?"> <incoming>Flow_Assessment_Exclusive</incoming> <outgoing>Flow_Assessment_OK_Repairs</outgoing> <outgoing>Flow_Assessment_NOK</outgoing> </exclusiveGateway> <sequenceFlow id="Flow_Assessment_OK_Repairs" sourceRef="ExclusiveGateway_AssessmentResult" targetRef="AuthorizeRepairsTask"/> <sequenceFlow id="Flow_Assessment_NOK" sourceRef="ExclusiveGateway_AssessmentResult" targetRef="RejectClaimTask"/> <!-- Phone garage to authorize repairs --> <task id="AuthorizeRepairsTask" name="Phone Garage (Authorize Repairs)"> <incoming>Flow_Assessment_OK_Repairs</incoming> <outgoing>Flow_AuthRepairs_SchedulePayment</outgoing> </task> <sequenceFlow id="Flow_AuthRepairs_SchedulePayment" sourceRef="AuthorizeRepairsTask" targetRef="SchedulePaymentTask"/> <!-- Schedule Payment --> <task id="SchedulePaymentTask" name="Schedule Payment"> <incoming>Flow_AuthRepairs_SchedulePayment</incoming> <outgoing>Flow_Payment_SendLetter</outgoing> </task> <sequenceFlow id="Flow_Payment_SendLetter" sourceRef="SchedulePaymentTask" targetRef="SendLetterTask"/> <!-- Send Letter (Final for all paths) --> <task id="SendLetterTask" name="Send Result Letter"> <incoming>Flow_Payment_SendLetter</incoming> <incoming>Flow_Reject_Letter</incoming> <outgoing>Flow_SendLetter_ThrowMsg</outgoing> </task> <sequenceFlow id="Flow_Reject_Letter" sourceRef="RejectClaimTask" targetRef="SendLetterTask"/> <intermediateThrowEvent id="MessageThrowEvent_Letter" name="Send Letter to Customer"> <incoming>Flow_SendLetter_ThrowMsg</incoming> <outgoing>Flow_LetterToCustomer</outgoing> <messageEventDefinition id="MessageEventDef_SendLetter"/> </intermediateThrowEvent> <sequenceFlow id="Flow_SendLetter_ThrowMsg" sourceRef="SendLetterTask" targetRef="MessageThrowEvent_Letter"/> <!-- Customer receives letter --> <intermediateCatchEvent id="MessageCatchEvent_Letter" name="Receive Letter"> <incoming>Flow_LetterToCustomer</incoming> <outgoing>Flow_Cust_EndEvent</outgoing> <messageEventDefinition id="MessageEventDef_ReceiveLetter"/> </intermediateCatchEvent> <!-- Message Flow: Insurer to Customer --> <sequenceFlow id="Flow_LetterToCustomer" sourceRef="MessageThrowEvent_Letter" targetRef="MessageCatchEvent_Letter"/> <endEvent id="EndEvent_Customer" name="End"> <incoming>Flow_Cust_EndEvent</incoming> </endEvent> <sequenceFlow id="Flow_Cust_EndEvent" sourceRef="MessageCatchEvent_Letter" targetRef="EndEvent_Customer"/> <!-- Insurer End Event --> <endEvent id="EndEvent_Insurer" name="End"> <incoming/> </endEvent> </process> 
  
